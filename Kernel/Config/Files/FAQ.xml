    <ConfigItem Name="LinkObject###FAQ" Required="1" Valid="1">
        <Description Lang="en">Link object settings for FAQ. Objects that are known by the system.</Description>
        <Description Lang="de">Link Objekt Einstellungen für die FAQ. Objekte, die dem System bekannt sind.</Description>
        <Group>FAQ</Group>
        <SubGroup>Core::LinkObject</SubGroup>
        <Setting>
            <Hash>
                <Item Key="Name">FAQ Object</Item>
                <Item Key="Type">Object</Item>
                <Item Key="LinkObjects">
                    <Array>
                        <Item>Ticket</Item>
                        <Item>FAQ</Item>
                    </Array>
                </Item>
            </Hash>
        </Setting>
    </ConfigItem>

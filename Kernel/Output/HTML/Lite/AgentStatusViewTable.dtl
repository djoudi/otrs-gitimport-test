<!-- start record -->
<tr valign="top">
 <td class="PriorityID-$Data{"PriorityID"}"><a href="$Env{"Baselink"}Action=AgentZoom&TicketID=$Data{"TicketID"}&QueueID=$Data{"QueueID"}" onmouseover="window.status='$Text{"Zoom"}'; return true;" onmouseout="window.status='';">$Data{"TicketNumber"}</a></td>
 <td class="PriorityID-$Data{"PriorityID"}">$Data{"Age"}</td>
 <td class="PriorityID-$Data{"PriorityID"}">$Data{"Subject"}&nbsp;</td>
 <td class="PriorityID-$Data{"PriorityID"}">$Data{"State"}</td>
 <td class="PriorityID-$Data{"PriorityID"}">$Data{"Queue"}</td>
 <td class="PriorityID-$Data{"PriorityID"}">$Data{"Owner"} ($Data{"UserFirstname"} $Data{"UserLastname"})</td>
 <td class="PriorityID-$Data{"PriorityID"}"><a href="$Env{"Baselink"}Action=AgentCustomer&TicketID=$Data{"TicketID"}" onmouseover="window.status='$Text{"Customer history"}'; return true;" onmouseout="window.status='';">$Data{"CustomerID"}</a>&nbsp;<br>$Data{"CustomerName"}</td>  
</tr>
<!-- end record -->

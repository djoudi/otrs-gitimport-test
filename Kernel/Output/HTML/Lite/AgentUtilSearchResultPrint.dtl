<!-- start form -->
<i>$Data{"Warning"}</i>

<table border="0" cellspacing="1" cellpadding="0" width="100%">
  <tr>
    <td><font size="+2">$Text{"Search"}</font></td>
  </td>
</table>

<table border="0" width="100%" cellspacing="0" cellpadding="4">
<tr> 
 <td class="table1" align="center" valign="top">
<table border="0" width="100%" cellspacing="0" cellpadding="3" cols="6">
<tr valign="top" align="left">
 <th width="15%">$Config{"TicketHook"}</th>
 <th width="15%">$Text{"Created"}</th>
 <th width="25%">$Text{"From"}/$Text{"Subject"}</th>
 <th width="15%">$Text{"State"}/$Text{"Queue"}</th>
 <th width="15%">$Text{"Owner"}</th>
 <th width="15%">$Text{"CustomerID"}</th>
</tr>
<!-- dtl:block:Record -->
<tr valign="top">
 <td>$Data{"TicketNumber"}</td>
 <td>$TimeLong{"$Data{"Created"}"}</td>
 <td>$QData{"From","30"}<br>$QData{"Subject","30"}</td>
 <td>$Quote{"$Text{"$Data{"State"}"}","30"}<br>$QData{"Queue","30"}</td>
 <td>$QData{"Owner","30"} ($Quote{"$Data{"UserFirstname"} $Data{"UserLastname"}","30"})</td>
 <td>$QData{"CustomerID","15"}&nbsp;<br>$QData{"CustomerName","15"}</td>
</tr>
<!-- dtl:block:Record -->
</table>

 </td>
</tr>
</table>
<!-- end form -->

<!-- start form -->
<table border="0" width="100%" cellspacing="0" cellpadding="3">
<tr>
  <td align="left" class="table0"">
    <b>$Env{"Box0"}$Text{"Ticket Search"}$Env{"Box1"}</b>
  </td>
</tr>
<tr align="center" valign="top">
  <td class="table1">

<table width="700" border="0" cellspacing="1" cellpadding="3">
<tr align="left">
  <td class="table3">$Text{"Profile"}</td>
</tr>
<tr align="left">
  <td>
  <form action="$Env{"CGIHandle"}" method="get">
  <input type="hidden" name="Action" value="AgentUtilities">
  <input type="hidden" name="Subaction" value="LoadProfile">
  $Text{"Search-Template"} $Data{"ProfilesStrg"}
  <input class="button" name="SelectTemplate" type="submit" value="$Text{"Select"}">
  <input class="button" name="EraseTemplate" type="submit" value="$Text{"Delete"}">
  <input class="button" name="SearchTemplate" type="submit" value="$Text{"Search"}">
  </form>
  </td>
</tr>
</table>


<form action="$Env{"CGIHandle"}" method="get">
<input type="hidden" name="Action" value="AgentUtilities">
<input type="hidden" name="Subaction" value="Search">

<table width="700" border="0" cellspacing="1" cellpadding="3">
<tr align="left">
  <td colspan="2" class="table3">$Config{"TicketHook"} $Text{"and"} $Text{"CustomerID"}</td>
</tr>
<tr align="left">
  <td width="50%">$Config{"TicketHook"} $Text{"(e. g. 10*5155 or 105658*)"}</td>
  <td width="50%"><input type="text" name="TicketNumber" size="30" value="$Data{"TicketNumber"}"></td>
</tr>
<tr align="left">
  <td>$Text{"CustomerID"} $Text{"(e. g. 234321)"}</td>
  <td><input type="text" name="CustomerID" size="30" value="$Data{"CustomerID"}"></td>
</tr>
<tr align="left">
  <td>$Text{"Customer User Login"} $Text{"(e. g. U5150)"}</td>
  <td><input type="text" name="CustomerUserLogin" size="30" value="$Data{"CustomerUserLogin"}"></td>
</tr>
<tr align="left">
  <td colspan="2" class="table3">$Text{"Fulltext-Search in Article (e. g. "Mar*in" or "Baue*")"}</td>
</tr>
<tr align="left">
  <td>$Text{"From"}</td>
  <td><input type="text" name="From" size="30" value="$Data{"From"}"></td>
</tr>
<tr align="left">
  <td>$Text{"To"}</td>
  <td><input type="text" name="To" size="30" value="$Data{"To"}"></td>
</tr>
<tr align="left">
  <td>$Text{"Cc"}</td>
  <td><input type="text" name="Cc" size="30" value="$Data{"Cc"}"></td>
</tr>
<tr align="left">
  <td>$Text{"Subject"}</td>
  <td><input type="text" name="Subject" size="30" value="$Data{"Subject"}"></td>
</tr>
<tr align="left">
  <td>$Text{"Text"}</td>
  <td><input type="text" name="Body" size="30" value="$Data{"Body"}"></td>
</tr>
<tr align="left">
  <td colspan="2" class="table3">$Text{"TicketFreeText"}</td>
</tr>
<tr align="left">
  <td>$Data{"TicketFreeKeyField1"}</td>
  <td>$Data{"TicketFreeTextField1"}</td>
</tr>
<tr align="left">
  <td>$Data{"TicketFreeKeyField2"}</td>
  <td>$Data{"TicketFreeTextField2"}</td>
</tr>
<tr align="left">
  <td>$Data{"TicketFreeKeyField3"}</td>
  <td>$Data{"TicketFreeTextField3"}</td>
</tr>
<tr align="left">
  <td>$Data{"TicketFreeKeyField4"}</td>
  <td>$Data{"TicketFreeTextField4"}</td>
</tr>
#<tr align="left">
#  <td>$Data{"TicketFreeKeyField5"}</td>
#  <td>$Data{"TicketFreeTextField5"}</td>
#</tr>

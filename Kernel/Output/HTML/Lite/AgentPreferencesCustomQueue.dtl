<!-- start form -->
<table border="0" width="100%" cellspacing="0" cellpadding="3" cols="1">
<tr align="center">
  <td class="contenthead">
    $Env{"Box0"}$Text{"$Config{"CustomQueue"}"}$Env{"Box1"}
  </td>
</tr>
<tr align="center">
  <td class="contentbody">
    <p>
    $Text{"Your queue selection of your favorite queues."}
    $Text{"You also get notified about this queues via email if enabled."}
    <form action="$Env{"CGIHandle"}" method="post">
    <input type="hidden" name="Action" value="$Env{"Action"}">
    <input type="hidden" name="Subaction" value="UpdateCustomQueues">
    $Data{"Option"}
    <br>
    <input class="button" type="submit" value="$Text{"update"}">
    </form>
    </p>
  </td>
</tr>
</table>
<!-- end form -->

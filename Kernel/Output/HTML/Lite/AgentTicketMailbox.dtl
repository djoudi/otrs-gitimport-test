<!-- dtl:block:NavBar -->
<table border="0" width="100%" cellspacing="0" cellpadding="3">
<tr>
  <td align="center" class="mainhead">
    $Env{"Box0"}$Text{"Mailbox"}: $Text{"$Data{"ViewType"}"}$Env{"Box1"}
  </td>
</tr>
<tr>
  <td class="menu">
  $Text{"Tickets"}:
  <a href="$Env{"Baselink"}Action=$Env{"Action"}&Subaction=All&SortBy=$Data{"SortBy"}&OrderBy=$Data{"OrderBy"}" onmouseover="window.status='$Text{"All messages"} ($Data{"All"})'; return true;" onmouseout="window.status='';">$Text{"All"} ($Data{"All"})</a>
#  -
#  <a href="$Env{"Baselink"}Action=$Env{"Action"}&Subaction=Open&SortBy=$Data{"SortBy"}&OrderBy=$Data{"OrderBy"}" onmouseover="window.status='$Text{"Open messages"} ($Data{"Open"})'; return true;" onmouseout="window.status='';">$Text{"Old messages"} ($Data{"Open"})</a>
  -
  <a href="$Env{"Baselink"}Action=$Env{"Action"}&Subaction=New&SortBy=$Data{"SortBy"}&OrderBy=$Data{"OrderBy"}" onmouseover="window.status='$Text{"New messages"} ($Data{"New"})'; return true;" onmouseout="window.status='';">$Text{"New messages"} ($Data{"New"})</a>
  -
  <a href="$Env{"Baselink"}Action=$Env{"Action"}&Subaction=Pending&SortBy=$Data{"SortBy"}&OrderBy=$Data{"OrderBy"}" onmouseover="window.status='$Text{"Pending messages"} ($Data{"Pending"})'; return true;" onmouseout="window.status='';">$Text{"Pending"} ($Data{"Pending"})</a>
  -
  <a href="$Env{"Baselink"}Action=$Env{"Action"}&Subaction=Reminder&SortBy=$Data{"SortBy"}&OrderBy=$Data{"OrderBy"}" onmouseover="window.status='$Text{"Reminder messages"} ($Data{"Reminder"})'; return true;" onmouseout="window.status='';">$Text{"Reminder"} ($Data{"Reminder"})</a>
  <br>
  $Text{"Sort by"}:
  <a href="$Env{"Baselink"}Action=$Env{"Action"}&Subaction=$Env{"Subaction"}&SortBy=Age">$Text{"Age"}</a> -
  <a href="$Env{"Baselink"}Action=$Env{"Action"}&Subaction=$Env{"Subaction"}&SortBy=Queue">$Text{"Queue"}</a> -
  <a href="$Env{"Baselink"}Action=$Env{"Action"}&Subaction=$Env{"Subaction"}&SortBy=Priority&OrderBy=Down">$Text{"Priority"}</a> -
  <a href="$Env{"Baselink"}Action=$Env{"Action"}&Subaction=$Env{"Subaction"}&SortBy=CustomerID">$Text{"CustomerID"}</a>
    | $Text{"Order"}:
  <a href="$Env{"Baselink"}Action=$Env{"Action"}&Subaction=$Env{"Subaction"}&SortBy=$Data{"SortBy"}&OrderBy=Up">$Text{"up"}</a> -
  <a href="$Env{"Baselink"}Action=$Env{"Action"}&Subaction=$Env{"Subaction"}&SortBy=$Data{"SortBy"}&OrderBy=Down">$Text{"down"}</a>
  </td>
</tr>
</table>
<!-- dtl:block:NavBar -->
<!-- dtl:block:Ticket -->
<table border="0" width="100%" cellspacing="0" cellpadding="3" cols="2">
<tr>

<!-- dtl:block:Bulk -->
  <td width="1%" class="mainhead">
    <form name="abulk$Data{"Counter"}"><input type="checkbox" name="Selected" value="1" onclick="BulkChangeSelected($Data{"TicketID"},$Data{"Counter"})"></form>
  </td>
<!-- dtl:block:Bulk -->

  <td align="left" width="65%" class="mainhead">
    $Env{"Box0"}$Config{"Ticket::Hook"}: $Data{"TicketNumber"}$Env{"Box1"}</b> <font color="red">$Text{"$Data{"Message"}"}
  </td>
  <td align="right" width="34%" class="mainhead">
    $Env{"Box0"}$Text{"Age"}: $Data{"Age"}$Env{"Box1"}
  </td>
</tr>
</table>
<table border="0" width="100%" cellspacing="0" cellpadding="3" cols="3">
<tr>
  <td width="65%" class="menu">

<!-- dtl:block:Menu -->
<!-- dtl:block:MenuItemSplit -->
 - 
<!-- dtl:block:MenuItemSplit -->
<!-- dtl:block:MenuItem -->
<a href="$Env{"Baselink"}$Data{"Link"}" $Data{"LinkParam"} onmouseover="window.status='$Text{"$QData{"Name"}"}'; return true;" onmouseout="window.status='';" class="menuitem" title="$Text{"$QData{"Description"}"}">
$Text{"$QData{"Name"}"}</a>
<!-- dtl:block:MenuItem -->
<!-- dtl:block:Menu -->

  </td>
  <td align="right" width="34%" class="menu">
    <table border="0" cellspacing="0" cellpadding="0">
      <tr>
        <td class="mainkey">$Text{"Created"}:</td>
        <td class="mainvalue"> $TimeLong{"$Data{"Created"}"}</td>
      </tr>
    </table>
  </td>
</tr>
</table>
<table border="0" width="100%" cellspacing="0" cellpadding="3" cols="2">
<tr>
  <td valign="top" width="60%" class="mainbody">
  <table border="0" width="100%" cellspacing="0" cellpadding="1">
    <tr>
      <td valign="top" width="15%"><b>$Text{"From"}:</b></td>
      <td width="85%"> <div title="$QData{"From"}">$QData{"From","80"}</div></td>
    </tr>
    <tr>
      <td valign="top"><b>$Text{"To"}:</b></td>
      <td> <div title="$QData{"To"}">$QData{"To","80"}</div></td>
    </tr>
# --
# show cc?
# --
    <dtl if ($Data{"Cc"} ne "") { $Data{"ArticleCcString"} = "<tr><td valign="top" ><b>$Text{"Cc"}:</b></td><td> <div title="$QData{"Cc"}">$QData{"Cc","80"}</div></td></tr>"; }>
    $Data{"ArticleCcString"}

    <tr>
      <td valign="top"><b>$Text{"Subject"}:</b></td>
      <td> <div title="$QData{"Subject"}">$QData{"Subject","80"}</div></td>
    </tr>
# --
# show ArticleFreeKey1-3 and ArticleFreeValue1-3
# --
<dtl if ($Data{"ArticleFreeKey1"} ne "") { $Data{"ArticleFreeString1"} = "<tr><td valign="top"><b>$Data{"ArticleFreeKey1"}:</b></td><td> $Data{"ArticleFreeValue1"}</td></tr>"; }>
<dtl if ($Data{"ArticleFreeKey2"} ne "") { $Data{"ArticleFreeString2"} = "<tr><td valign="top"><b>$Data{"ArticleFreeKey2"}:</b></td><td> $Data{"ArticleFreeValue2"}</td></tr>"; }>
<dtl if ($Data{"ArticleFreeKey3"} ne "") { $Data{"ArticleFreeString3"} = "<tr><td valign="top"><b>$Data{"ArticleFreeKey3"}:</b></td><td> $Data{"ArticleFreeValue3"}</td></tr>"; }>
     $Data{"ArticleFreeString1"}
     $Data{"ArticleFreeString2"}
     $Data{"ArticleFreeString3"}
# --
  </table>
  </td> 
  <td width="20%" align="left" valign="top" class="PriorityID-$Data{"PriorityID"}">
    <table border="0">
    <tr valign="top">
      <td><b>$Text{"State"}:</b></td>
      <td><font color="red"><div title="$Quote{"$Text{"$Data{"State"}"}"}">$Quote{"$Text{"$Data{"State"}"}","14"}</div></font></td>
    </tr>
    <tr valign="top">
      <td><b>$Text{"Priority"}:</b></td>
      <td><font color="red"><div title="$Quote{"$Text{"$Data{"Priority"}"}"}">$Quote{"$Text{"$Data{"Priority"}"}","14"}</div></font></td>
    </tr>
    <tr valign="top">
      <td><b>$Text{"Queue"}:</b></td>
      <td><font color="red"><div title="$Quote{"$Data{"Queue"}"}">$Quote{"$Data{"Queue"}","14"}</div></font></td>
    </tr>
    </table>
  </td>
  <td width="20%" align="left" valign="top" class="PriorityID-$Data{"PriorityID"}">
    <table border="0">
    <tr valign="top">
      <td><b>$Text{"CustomerID"}:</b></td>
      <td><a href="$Config{"CustomerDBLink"}" $Config{"CustomerDBLinkTarget"}>$Quote{"$Data{"CustomerID"}","14"}</a></td>
    </tr>
# --
# use ticket is pending 
# --
<dtl if ($Data{"PendingUntil"} ne "") { $Data{"PendingUntilString"} = "<tr valign="top"><td><b>$Text{"Pending till"}:</b></td><td>$Data{"PendingUntil"}</td></tr>"; }>
    $Data{"PendingUntilString"}
# --
# show TicketFreeKey1-2 and TicketFreeText1-2
# --
<dtl if ($Data{"TicketFreeKey1"} ne "") { $Data{"TicketFreeString1"} = "<tr valign="top"><td><b>$Data{"TicketFreeKey1"}:</b></td><td>$Data{"TicketFreeText1"}</td></tr>"; }>
<dtl if ($Data{"TicketFreeKey2"} ne "") { $Data{"TicketFreeString2"} = "<tr valign="top"><td><b>$Data{"TicketFreeKey2"}:</b></td><td>$Data{"TicketFreeText2"}</td></tr>"; }>
    $Data{"TicketFreeString1"}
    $Data{"TicketFreeString2"}
# --
    </table>
  </td>
</tr>
</table>
<!-- dtl:block:Ticket -->

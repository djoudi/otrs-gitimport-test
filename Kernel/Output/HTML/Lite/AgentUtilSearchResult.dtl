<!-- start form -->
<table border="0" width="100%" cellspacing="0" cellpadding="3" cols="2">

<tr bgcolor="$Env{"BGTableColor0"}">
  <td align="left">
  <b><font color="$Env{"FontColor1"}">$Env{"Box0"}$Config{"TicketHook"}: $Data{"TicketNumber"}$Env{"Box1"}</font></b>
  </td>
  <td align="right">
  <b><font color="$Env{"FontColor1"}">$Env{"Box0"}$Text{"Age"}: $Data{"Age"}$Env{"Box1"}</font></b></font></b>
  </td>
</tr>

<tr bgcolor="$Env{"BGTableColor2"}">
  <td align="left" valign="top">
  <b>$Text{"State"}:</b> $Data{"State"} -
  <b>$Text{"Queue"}:</b> $Data{"Queue"} 
  <br>
  <b>$Text{"Owner"}:</b> $Data{"Owner"} -
  <b>$Text{"Lock"}:</b> $Data{"Lock"} 
  </td>

  <td align="right" valign="top">
  <a href="$Env{"Baselink"}&Action=AgentZoom&TicketID=$Data{"TicketID"}&QueueID=$Data{"QueueID"}">$Text{"Zoom"}</a> -
  <a href="$Env{"Baselink"}&Action=AgentHistory&TicketID=$Data{"TicketID"}&QueueID=$Data{"QueueID"}">$Text{"History"}</a> 
  </td>
</tr>

<tr bgcolor="$Env{"BGTableColor1"}">
  <td align="left" valign="top" colspan="2">
<b>$Text{"From"}:</b> $Data{"From"} 
<br>
<b>$Text{"Subject"}:</b> $Data{"Subject"}
<br>
<pre>
$Data{"Text"}
</pre>
  </td> 
</tr>
</table>
<!-- end form -->


<!-- start form -->
<table border="0" width="100%" cellspacing="0" cellpadding="3" cols="1">
<tr>
  <td align="left" class="table0">
    <b>$Env{"Box0"}$Text{"Change customer of ticket"}: $Data{"TicketNumber"}$Env{"Box1"}</b>
  </td>
</tr>
<tr>
  <td align="left" class="table2">
    <a href="$Env{"Baselink"}$Env{"LastScreen"}" onmouseover="window.status='$Text{"Back"}'; return true;" onmouseout="window.status='';">$Text{"Back"}</a>
  </td>
</tr>
</table>
<table border="0" width="100%" cellspacing="0" cellpadding="3" cols="1">
<tr>
  <td align="center" valign="top" class="table1">

<p>
$Text{"Set customer id of a ticket"}:
<br>
<form action="$Env{"CGIHandle"}" method="post">
<input type="hidden" name="Action" value="$Env{"Action"}">
<input type="hidden" name="Subaction" value="Update">
<input type="hidden" name="QueueID" value="$Data{"QueueID"}">
<input type="hidden" name="TicketID" value="$Data{"TicketID"}">
<input type="hidden" name="NextScreen" value="$Data{"NextScreen"}">
$Data{"CustomerStrg"}
<input type="text" name="CustomerID" value="$Data{"CustomerID"}"> 
<br>
<input type="submit" value="$Text{"update"}">
</form>

</p>

  </td> 
</tr>
</table>
<!-- end form -->

<!-- start form -->
<table border="0" width="100%" cellspacing="0" cellpadding="3">
<tr>
  <td class="mainhead">
    $Env{"Box0"}$Text{"Customer history search"}$Env{"Box1"}
  </td>
</tr>
<tr>
  <td align="center" class="mainbody">

<form action="$Env{"CGIHandle"}" method="post">
<table width="700" border="0" cellspacing="0" cellpadding="3">
<tr>
  <td class="contenthead">$Text{"Customer history search (e. g. "ID342425")."} $Text{"No * possible!"}</td>
</tr>
<tr>
  <td class="contentbody">
    <font color="red">$Text{"$Data{"Message"}"}</font>
    <input type="hidden" name="Action" value="AgentCustomer">
    <input type="hidden" name="Subaction" value="CustomerID">
    <input type="hidden" name="Search" value="1">
    $Text{"CustomerID"} <input type="text" name="CustomerID" size="20" value="$QData{"CustomerID"}">
  </td>
</tr>
<tr>
  <td class="contentfooter">
    <input class="button" type="submit" value="$Text{"Search"}">
  </td>
</tr>
</table>
</form>

  </td>
</tr>
</table>
<!-- end form -->

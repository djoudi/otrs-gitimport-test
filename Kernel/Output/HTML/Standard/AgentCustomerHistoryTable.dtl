<!-- start recorde -->
<tr>
  <td align="center"><a href="$Env{"Baselink"}Action=AgentZoom&TicketID=$Data{"TicketID"}&QueueID=$Data{"QueueID"}" onmouseover="window.status='$Text{"Zoom"}'; return true;" onmouseout="window.status='';">$Data{"TicketNumber"}</a></td>
  <td>$Text{"From"}: $Quote{"$Data{"From"}","20"}<br>$Text{"Subject"}: $Quote{"$Data{"Subject"}","16"}</td>
  <td>$Text{"State"}: $Quote{"$Data{"State"}","20"}<br>$Text{"Lock"}: $Quote{"$Data{"Lock"}","16"}</td>
  <td>$Text{"Queue"}: $Quote{"$Data{"Queue"}","20"}<br>$Text{"Priority"}: $Quote{"$Data{"Priority"}","16"}</td>
  <td>$Data{"Age"}</td>
</tr>
<!-- end recorde -->

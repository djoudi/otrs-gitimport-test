<!-- dtl:block:ContentSmallUserOnlineFilter -->
<div>
    <a href="#" onclick="Core.AJAXContentUpdate('Dashboard$QData{"Name"}', '$Env{"Baselink"}Action=$Env{"Action"};Subaction=Element;Name=$QData{"Name"};Filter=Agent'); return false;" style="$QData{"Agent::Style"}">$Text{"Agents"} ($QData{"Agent"})</a>
    |
    <a href="#" onclick="Core.AJAXContentUpdate('Dashboard$QData{"Name"}', '$Env{"Baselink"}Action=$Env{"Action"};Subaction=Element;Name=$QData{"Name"};Filter=Customer'); return false;" style="$QData{"Customer::Style"}">$Text{"Customers"} ($QData{"Customer"})</a>
<!-- dtl:block:ContentSmallTicketGenericFilterNavBar -->
    <div class="Pagination">
        $Data{"SiteNavBar"}
    </div>
<!-- dtl:block:ContentSmallTicketGenericFilterNavBar -->
</div>
<!-- dtl:block:ContentSmallUserOnlineFilter -->
<table class="DataTable">
    <tbody>
<!-- dtl:block:ContentSmallUserOnlineRow -->
        <tr>
            <td colspan="2">
                $QData{"UserLastname"} $QData{"UserFirstname"}
<!-- dtl:block:ContentSmallUserOnlineRowEmail -->
                ($QData{"UserEmail","50"})
<!-- dtl:block:ContentSmallUserOnlineRowEmail -->
            </td>
        </tr>
<!-- dtl:block:ContentSmallUserOnlineRow -->
<!-- dtl:block:ContentSmallUserOnlineNone -->
        <tr>
            <td colspan="2">
                $Text{"none"}
            </td>
        </tr>
<!-- dtl:block:ContentSmallUserOnlineNone -->
    </tbody>
</table>

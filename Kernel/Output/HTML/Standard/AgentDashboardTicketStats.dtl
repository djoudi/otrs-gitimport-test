<script type="text/javascript" src="$Config{"Frontend::JavaScriptPath"}thirdparty/swfobject.js"></script>
<script type="text/javascript">
    swfobject.embedSWF("$Config{"Frontend::ImagePath"}open-flash-chart.swf", "chart$QData{"Key"}", "320", "200", "9.0.0");

    function open_flash_chart_data()
    {
        var data = '$Data{"Source"}';
        return data;
    }

    function findSWF(movieName) {
        if (navigator.appName.indexOf("Microsoft")!= -1) {
            return window[movieName];
        }
        else {
            return document[movieName];
        }
    }
</script>
<div style="text-align:center; padding: 5px; background-color: #FFFFFF;">
    <div id="chart$QData{"Key"}"><p>$Config{"Frontend::JavaScriptPath"} - Unable to load Flash content. The Charts Control requires Flash Player 9 or higher. You can download the latest version of Flash Player from the <a href="http://www.adobe.com/go/getflashplayer">Adobe Flash Player Download Center</a>.</p></div>
</div>
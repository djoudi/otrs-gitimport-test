<!--dtl:block:rw:PendingTime-->
<label>$Data{"Label"}:</label>
<div class="Field">
    $Data{"Content"}
    <div id="PendingTimeDayError" class="TooltipErrorMessage"><p>$Data{"PendingtTimeDayError"}</p></div>
    <div id="PendingTimeHourError" class="TooltipErrorMessage"><p>$Data{"PendingtTimeHourError"}</p></div>
<!-- dtl:block:rw:Owner:DescriptionShort -->

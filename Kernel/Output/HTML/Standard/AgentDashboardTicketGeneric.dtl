<!-- dtl:block:ContentLargeTicketGenericFilter -->
<div class="TicketFilter">
    <a href="#" id="Dashboard$QData{"Name"}Locked" class="$QData{"Locked::Selected"}">$Text{"Locked Tickets"} ($QData{"Locked"})</a>
<!-- dtl:js_on_document_complete -->
<script type="text/javascript">//<![CDATA[
    OTRS.UI.RegisterLiveEvent('click', '#Dashboard$QData{"Name"}Locked', function(){
        OTRS.AJAX.ContentUpdate($('#Dashboard$QData{"Name"}'), '$Env{"Baselink"}Action=$Env{"Action"};Subaction=Element;Name=$QData{"Name"};Filter=Locked', function () {
            OTRS.UI.Tables.InitCSSPseudoClasses($('#Dashboard$QData{"Name"}'));
        });
        return false;
    });
//]]></script>

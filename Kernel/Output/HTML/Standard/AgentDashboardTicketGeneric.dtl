<!-- dtl:block:ContentLargeTicketGenericFilter -->
<div>
    <a href="#" onclick="Core.AJAXContentUpdate('Dashboard$QData{"Name"}', '$Env{"Baselink"}Action=$Env{"Action"};Subaction=Element;Name=$QData{"Name"};Filter=Locked'); return false;" style="$QData{"Locked::Style"}">$Text{"Locked Tickets"} ($QData{"Locked"})</a>
<!-- dtl:block:ContentLargeTicketGenericFilterWatcher -->
    | <a href="#" onclick="Core.AJAXContentUpdate('Dashboard$QData{"Name"}', '$Env{"Baselink"}Action=$Env{"Action"};Subaction=Element;Name=$QData{"Name"};Filter=Watcher'); return false;" style="$QData{"Watcher::Style"}">$Text{"Watched"} ($QData{"Watcher"})</a>
<!-- dtl:block:ContentLargeTicketGenericFilterWatcher -->
<!-- dtl:block:ContentLargeTicketGenericFilterResponsible -->
    | <a href="#" onclick="Core.AJAXContentUpdate('Dashboard$QData{"Name"}', '$Env{"Baselink"}Action=$Env{"Action"};Subaction=Element;Name=$QData{"Name"};Filter=Responsible'); return false;" style="$QData{"Responsible::Style"}">$Text{"Responsible"} ($QData{"Responsible"})</a>
<!-- dtl:block:ContentLargeTicketGenericFilterResponsible -->
    | <a href="#" onclick="Core.AJAXContentUpdate('Dashboard$QData{"Name"}', '$Env{"Baselink"}Action=$Env{"Action"};Subaction=Element;Name=$QData{"Name"};Filter=MyQueues'); return false;" style="$QData{"MyQueues::Style"}">$Text{"My Queues"} ($QData{"MyQueues"})</a>
    | <a href="#" onclick="Core.AJAXContentUpdate('Dashboard$QData{"Name"}', '$Env{"Baselink"}Action=$Env{"Action"};Subaction=Element;Name=$QData{"Name"};Filter=All'); return false;" style="$QData{"All::Style"}">$Text{"All"} ($QData{"All"})</a>

<!-- dtl:block:ContentLargeTicketGenericFilterNavBar -->
    <div class="Pagination">
        $Data{"SiteNavBar"}
    </div>
<!-- dtl:block:ContentLargeTicketGenericFilterNavBar -->
</div>
<!-- dtl:block:ContentLargeTicketGenericFilter -->
<table class="DataTable">
    <tbody>
<!-- dtl:block:ContentLargeTicketGenericRow -->
        <tr class="">
<!-- dtl:block:ContentLargeTicketGenericRowMeta -->
            <td class="$QData{"ClassTable"} Fixed" title="$Text{"$Data{"Title"}"}">
<!-- dtl:block:ContentLargeTicketGenericRowMetaImage -->
#                <img border="0" src="$Config{"Frontend::ImagePath"}$QData{"Image"}" alt="" title="$Text{"$QData{"Title"}"}"/>
                <div class="$QData{"Class"} Small" title="$Text{"$Data{"Title"}"}">
                    <span class="$QData{"ClassSpan"}">$Text{"$Data{"Title"}"}</span>
                </div>
<!-- dtl:block:ContentLargeTicketGenericRowMetaImage -->
            </td>
<!-- dtl:block:ContentLargeTicketGenericRowMeta -->
            <td width="10%">
                <a href="$Env{"Baselink"}Action=AgentTicketZoom;TicketID=$Data{"TicketID"}" title="$QData{"Title"}" class="AsBlock">$Data{"TicketNumber"}</a>
            </td>
            <td width="55%">
                <div title="$QData{"Title"}">$QData{"Title","70"}</div>
            </td>
            <td>$QData{"Time"}</td>
        </tr>
<!-- dtl:block:ContentLargeTicketGenericRow -->
<!-- dtl:block:ContentLargeTicketGenericNone -->
        <tr>
            <td>
                $Text{"none"}
            </td>
        </tr>
<!-- dtl:block:ContentLargeTicketGenericNone -->
    </tbody>
</table>

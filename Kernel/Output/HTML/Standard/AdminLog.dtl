<div class="MainBox ARIARoleMain LayoutFixedSidebar SidebarFirst">
    <h1>$Text{"System Log"}</h1>

    <div class="SidebarColumn">
        <div class="WidgetGradient Short MinHeight">
            <div class="Header">
                <h2>$Text{"Note"}</h2>
            </div>
            <div class="Content">
                <p class="FieldExplanation">
                    $Text{"Here you will find log information about your system."}
                </p>
            </div>
        </div>
    </div>
    <div class="ContentColumn">

        <div class="WidgetGradient Short MinHeight">
            <div class="Header"></div>
            <div class="Content">
                <table class="DataTable">
                    <thead>
                        <tr>
                            <th>$Text{"Time"}</th>
                            <th>$Text{"Priority"}</th>
                            <th>$Text{"Facility"}</th>
                            <th class="W75pc">$Text{"Message"}</th>
                        </tr>
                    </thead>
                    <tbody>
<!-- dtl:block:Row -->
                        <tr class="$QData{"ErrorClass"}">
                            <td>$QData{"Time"}</td>
                            <td>$QData{"Priority"}</td>
                            <td>$QData{"Facility"}</td>
                            <td>$QData{"Message"}</td>
                        </tr>
<!-- dtl:block:Row -->
                    </tbody>
                </table>
            </div>
        </div>
    </div>
    <div class="Clear"></div>
</div>

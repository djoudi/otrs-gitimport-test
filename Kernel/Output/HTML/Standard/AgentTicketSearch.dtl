<!-- dtl:block:Search -->
<table border="0" width="100%" cellspacing="0" cellpadding="3">
    <tr>
        <td class="mainhead">
            $Env{"Box0"}$Text{"Ticket Search"}$Env{"Box1"}
        </td>
    </tr>
    <tr>
        <td align="center" class="mainbody">
            <br/>
            <form action="$Env{"CGIHandle"}" method="get">
                <input type="hidden" name="Action" value="$Env{"Action"}"/>
                <input type="hidden" name="Subaction" value="LoadProfile"/>
                <table width="800" border="0" cellspacing="0" cellpadding="3">
                    <tr>
                        <td class="contenthead">$Text{"Profile"}</td>
                    </tr>
                    <tr>
                        <td class="contentbody">
                            <table border="0" cellspacing="0" cellpadding="3">
                                <tr>
                                    <td class="contentkey">
                                        $Text{"Search-Template"}
                                    </td>
                                    <td class="contentvalue">
                                        $Data{"ProfilesStrg"}
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                    <tr>
                        <td class="contentfooter">
                            <button class="button" name="SelectTemplate" type="submit" value="$Text{"Select"}">$Text{"Select"}</button>
                            <button class="button" name="EraseTemplate" type="submit" value="$Text{"Delete"}">$Text{"Delete"}</button>
                            <button class="button" name="SearchTemplate" type="submit" value="$Text{"Search"}">$Text{"Search"}</button>
                        </td>
                    </tr>
                </table>
            </form>
            <br/>
            <form action="$Env{"CGIHandle"}" method="post" name="compose">
                <input type="hidden" name="Action" value="$Env{"Action"}"/>
                <input type="hidden" name="Subaction" value="Search"/>

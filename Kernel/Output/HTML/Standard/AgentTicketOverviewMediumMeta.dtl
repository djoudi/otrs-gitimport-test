<!-- dtl:block:TicketHeader -->
<script language="JavaScript" type="text/javascript">
function select_items (Type) {
    for (var i = 1; i < document.matrix.elements[Type].length; i++) {
        if ( document.matrix.elements[Type][0].checked ) {
            document.matrix.elements[Type][i].checked = true;
        }
        else {
            document.matrix.elements[Type][i].checked = false;
        }
    }
}
function select_item (Type, TicketID) {
    if ( document.matrix.TicketID[Type].value ) {
        document.matrix.TicketID[Type].value = '';
    }
    else {
        document.matrix.TicketID[Type].value = TicketID;
    }
}
function select_item_check () {
    for (var i = 1; i < document.matrix.TicketID.length; i++) {
        if ( document.matrix.TicketID[i].checked ) {
            return true;
        }
    }
    alert('$JSText{"You need min. one selected Ticket!"}');
    return false;
}
</script>
<!-- dtl:block:TicketHeader -->


<!-- start form -->
<!-- dtl:block:WindowTabStart -->
<form action="$Env{"CGIHandle"}" method="post" enctype="multipart/form-data">
    <input type="hidden" name="Action" value="$Env{"Action"}"/>
    <table border="0" width="100%" cellspacing="0" cellpadding="3">
        <tr>
            <td class="mainbody">
                <br/>
                <table border="0" cellpadding="0" cellspacing="0" width="99%" align="center">
                    <tr>
                        <td class="tablayer0">
                            <table border="0" cellpadding="0" cellspacing="0" width="100%">
                                <tr>
<!-- dtl:block:Tab -->
                                    <td width="$QData{"Size"}%">
                                        <div align="center">
                                            <table border="0" width="95%" cellpadding="3" cellspacing="0">
                                                <tr>
                                                    <td class="tab$Data{"Active"}">
                                                        <p align="center">
                                                            <button class="tab$QData{"Active"}" type="submit" value="$Text{"$Data{"Text"}"}" name="$QData{"Name"}">$Text{"$Data{"Text"}"}</button>
                                                            <img border="0" src="$Config{"Frontend::ImagePath"}$Data{"Image"}" />
                                                        </p>
                                                    </td>
                                                </tr>
                                            </table>
                                        </div>
                                    </td>
<!-- dtl:block:Tab -->
                                </tr>
                            </table>
                            <table border="0" cellpadding="0" cellspacing="0" width="100%">
                                <tr>
                                    <td class="tablayer1">&nbsp;<br/>&nbsp;
                                        <div align="center">
<!-- dtl:block:WindowTabStart -->
<!-- dtl:block:WindowTabStop -->
                                        </div>
                                        <br/>
                                        <table border="0" cellpadding="5" cellspacing="0" width="100%">
                                            <tr>
                                                <td>&nbsp;</td>
                                                <td align="right">
<!-- dtl:block:Layer1Footer -->
                                                    <button class="button" type="submit" value="$Text{"$Data{"Text"}"}" name="$QData{"Name"}">$Text{"$Data{"Text"}"}</button>
<!-- dtl:block:Layer1Footer -->
                                                </td>
                                            </tr>
                                        </table>
                                    </td>
                                </tr>
                            </table>
                            <table border="0" cellpadding="5" cellspacing="0" width="100%">
                                <tr>
<!-- dtl:block:Layer0Footer -->
                                    <td>
                                        <button class="button" type="submit" value="$Text{"$Data{"Text"}"}" name="$QData{"Name"}">$Text{"$Data{"Text"}"}</button>
                                    </td>
<!-- dtl:block:Layer0Footer -->
                                </tr>
                            </table>
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
        <tr>
            <td align="left" class="mainbody">
<!-- dtl:block:Required -->
                <div class="required">* $Text{"$Data{"Description"}"}</div>
<!-- dtl:block:Required -->
<!-- dtl:block:Readonly -->
                <div class="readonly">* $Text{"$Data{"Description"}"}</div>
<!-- dtl:block:Readonly -->
<!-- dtl:block:Footnote -->
                <div class="footnote">* $Text{"$Data{"Description"}"}</div>
<!-- dtl:block:Footnote -->
            </td>
        </tr>
        <tr class="mainbody">
            <td><br/></td>
        </tr>
    </table>
</form>
<!-- dtl:block:WindowTabStop -->

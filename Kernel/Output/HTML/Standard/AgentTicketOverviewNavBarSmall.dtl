<!-- dtl:block:OverviewNavBar -->
<table border="0" width="100%" cellspacing="0" cellpadding="3">
    <tr>
        <td class="menu">
            <table border="0" width="100%" cellspacing="0" cellpadding="0">
                <tr>
                    <td width="90%">
                        <table border="0" width="100%" cellspacing="0" cellpadding="0">
                            <tr>
                                <td width="100%">
<!-- dtl:block:OverviewNavBarPageNavBar -->
                                    $Text{"Tickets"}: $Data{"Result"} $Text{"of"} $Data{"TotalHits"} -
                                    $Text{"Page"}: $Data{"SiteNavBar"}
<!-- dtl:block:OverviewNavBarPageNavBar -->
                                </td>
                            </tr>
                            <tr>
                                <td width="100%">
<!-- dtl:block:OverviewNavBarFilter -->
                                    $Text{"Filter"}:
<!-- dtl:block:OverviewNavBarFilterItem -->
<!-- dtl:block:OverviewNavBarFilterItemSelected -->
                                    <a name="OverviewControl" href="$Env{"Baselink"}Action=$Env{"Action"};$Data{"FilterLink"}Filter=$LQData{"Filter"}"><b>$Text{"$QData{"Name"}"} ($QData{"Count"})</b></a>
<!-- dtl:block:OverviewNavBarFilterItemSelected -->
<!-- dtl:block:OverviewNavBarFilterItemSelectedNot -->
                                    <a name="OverviewControl" href="$Env{"Baselink"}Action=$Env{"Action"};$Data{"FilterLink"}Filter=$LQData{"Filter"}">$Text{"$QData{"Name"}"} ($QData{"Count"})</a>
<!-- dtl:block:OverviewNavBarFilterItemSelectedNot -->
<!-- dtl:block:OverviewNavBarFilterItemSplit -->
                                    -
<!-- dtl:block:OverviewNavBarFilterItemSplit -->
<!-- dtl:block:OverviewNavBarFilterItem -->
<!-- dtl:block:OverviewNavBarFilter -->
                                </td>
                            </tr>
                        </table>
                    </td>
                    <td width="20%" align="right" valign="top">
<!-- dtl:block:OverviewNavBarViewMode -->
<!-- dtl:block:OverviewNavBarViewModeSelected -->
                        <a name="OverviewControl" href="$Env{"Baselink"}Action=$Env{"Action"};Filter=$LQData{"Filter"};View=$LQData{"View"}#subnav" title="$QData{"Name"}"><img border="0" src="$Config{"Frontend::ImagePath"}$QData{"ImageSelected"}" title="$Text{"$QData{"Name"}"} alt="$Text{"$QData{"Name"}"}" /></a>
<!-- dtl:block:OverviewNavBarViewModeSelected -->
<!-- dtl:block:OverviewNavBarViewModeNotSelected -->
                        <a name="OverviewControl" href="$Env{"Baselink"}Action=$Env{"Action"};Filter=$LQData{"Filter"};View=$LQData{"View"}#subnav" title="$QData{"Name"}"><img border="0" src="$Config{"Frontend::ImagePath"}$QData{"Image"}" title="$Text{"$QData{"Name"}"}" alt="$Text{"$QData{"Name"}"}" /></a>
<!-- dtl:block:OverviewNavBarViewModeNotSelected -->
<!-- dtl:block:OverviewNavBarViewMode -->
                        &nbsp;
                    </td>
                </tr>
            </table>
        </td>
    </tr>
<!-- dtl:block:OverviewNavBarMain -->
    <tr>
        <td class="mainbody">
            $Text{"$QData{"MainName"}"}: $Data{"MainContent"}
        </td>
    </tr>
<!-- dtl:block:OverviewNavBarMain -->
</table>
<!-- dtl:block:OverviewNavBar -->

<!-- view settings start -->
$Include{"AJAX"}
<link rel="stylesheet" type="text/css" href="$Config{"Frontend::CSSPath"}overcast/jquery-ui.css" />
<script type="text/javascript">//<![CDATA[
    OTRSUI.Dialog(
        {
            Document: document,
            Selector: '#DialogSet',
            Dialog:   '#DialogSettings',
            Form:     '#DialogSettingsForm',
            Modal:    true,
            Title:    '$JSText{"Settings"}',
            Type:     'Confirm',
            Translation: {
                Cancel: '$JSText{"Cancel"}',
                Submit: '$JSText{"Submit"}'
            }
        }
    );
//]]></script>
    <div id="DialogSettings" style="display: none;">
        <div class="bd">
            <form action="$Env{"CGIHandle"}" method="post" id="DialogSettingsForm" name="DialogSettingsForm">
                <input type="hidden" name="Action" value="AgentPreferences"/>
                <input type="hidden" name="Subaction" value="Update"/>
                <input type="hidden" name="Group" value="$QData{"Group"}"/>
                <input type="hidden" name="RedirectURL" value="$QData{"RequestedURL"}"/>
                <table>
                    <tr valign="top" align="left">
                        <td>
                            <label for="$QData{"PreferencesKey"}"><b>$Text{"Max. shown Tickets a page"}:</b></label>
                            $Data{"PageShownString"}
                        </td>
                    </tr>
                </table>
            </form>
        </div>
    </div>
<!-- view settings end -->

    <a name="subnav"></a>
<!-- dtl:block:OverviewNavBar -->
    <table border="0" width="100%" cellspacing="0" cellpadding="3">
        <tr>
            <td class="mainhead">
                $Env{"Box0"}$Text{"$QData{"TitleName"}"}: $Text{"$QData{"TitleValue"}"}$Env{"Box1"}
            </td>
        </tr>
<!-- dtl:block:OverviewNavBarMain -->
        <tr>
            <td class="menu">
                $Text{"$QData{"MainName"}"}: $Data{"MainContent"}
            </td>
        </tr>
<!-- dtl:block:OverviewNavBarMain -->
        <tr>
            <td class="menu">
                <table border="0" width="100%" cellspacing="0" cellpadding="0">
                    <tr>
                        <td width="90%">
                            <table border="0" width="100%" cellspacing="0" cellpadding="0">
                                <tr>
                                    <td width="100%">
<!-- dtl:block:OverviewNavBarFilter -->
                                        $Text{"Filter"}:
<!-- dtl:block:OverviewNavBarFilterItem -->
<!-- dtl:block:OverviewNavBarFilterItemSelected -->
                                        <a name="OverviewControl" href="$Env{"Baselink"}Action=$Env{"Action"};$Data{"FilterLink"}Filter=$LQData{"Filter"}"><b>$Text{"$QData{"Name"}"} ($QData{"Count"})</b></a>
<!-- dtl:block:OverviewNavBarFilterItemSelected -->
<!-- dtl:block:OverviewNavBarFilterItemSelectedNot -->
                                        <a name="OverviewControl" href="$Env{"Baselink"}Action=$Env{"Action"};$Data{"FilterLink"}Filter=$LQData{"Filter"}">$Text{"$QData{"Name"}"} ($QData{"Count"})</a>
<!-- dtl:block:OverviewNavBarFilterItemSelectedNot -->
<!-- dtl:block:OverviewNavBarFilterItemSplit -->
                                        -
<!-- dtl:block:OverviewNavBarFilterItemSplit -->
<!-- dtl:block:OverviewNavBarFilterItem -->
<!-- dtl:block:OverviewNavBarFilter -->
                                    </td>
                                </tr>
                                <tr>
                                    <td width="100%">
<!-- dtl:block:OverviewNavBarPageBack -->
                                        <a name="OverviewControl" href="$Env{"Baselink"}Action=$Env{"Action"};$Data{"LinkBack"}">$Text{"Change search options"}</a>
<!-- dtl:block:OverviewNavBarPageBack -->
<!-- dtl:block:OverviewNavBarPageNavBar -->
                                        $Text{"Tickets"}: $Data{"Result"} $Text{"of"} $Data{"TotalHits"} -
                                        $Text{"Page"}: $Data{"SiteNavBar"}
<!-- dtl:block:OverviewNavBarPageNavBar -->
                                    </td>
                                </tr>
                            </table>
                        </td>
                        <td width="20%" align="right" valign="top">

<!-- view settings start -->
                            <a href="" id="DialogSet" onclick="return false;" title="$Text{"Overview Settings"}"><img border="0" src="$Config{"Frontend::ImagePath"}edit-inactive.png" /></a>
                            <img src="$Config{"Frontend::ImagePath"}h-line-small.png" />
<!-- view settings end -->

<!-- dtl:block:OverviewNavBarViewMode -->
<!-- dtl:block:OverviewNavBarViewModeSelected -->
                            <a name="OverviewControl" href="$Env{"Baselink"}Action=$Env{"Action"};Filter=$LQData{"Filter"};View=$LQData{"View"};$Data{"LinkFilter"}#subnav" title="$Text{"$QData{"Name"}"}"><img border="0" src="$Config{"Frontend::ImagePath"}$QData{"ImageSelected"}" title="$Text{"$QData{"Name"}"}" alt="$Text{"$QData{"Name"}"}" /></a>
<!-- dtl:block:OverviewNavBarViewModeSelected -->
<!-- dtl:block:OverviewNavBarViewModeNotSelected -->
                            <a name="OverviewControl" href="$Env{"Baselink"}Action=$Env{"Action"};Filter=$LQData{"Filter"};View=$LQData{"View"};$Data{"LinkFilter"}#subnav" title="$Text{"$QData{"Name"}"}"><img border="0" src="$Config{"Frontend::ImagePath"}$QData{"Image"}" title="$Text{"$QData{"Name"}"}" alt="$Text{"$QData{"Name"}"}" /></a>
<!-- dtl:block:OverviewNavBarViewModeNotSelected -->
<!-- dtl:block:OverviewNavBarViewMode -->
                            &nbsp;
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
    </table>
<!-- dtl:block:OverviewNavBar -->

<!-- dtl:block:DISABLE -->
<!-- view settings start -->
$Include{"AJAX"}
<link rel="stylesheet" type="text/css" href="$Config{"Frontend::CSSPath"}overcast/jquery-ui.css" />
<script type="text/javascript">//<![CDATA[
    UI.Dialog(
        {
            Document: document,
            Selector: '#DialogSet',
            Dialog:   '#DialogSettings',
            Form:     '#DialogSettingsForm',
            Modal:    true,
            Title:    '$JSText{"Settings"}',
            Type:     'Confirm',
            Translation: {
                Cancel: '$JSText{"Cancel"}',
                Submit: '$JSText{"Submit"}'
            }
        }
    );
//]]></script>
<div id="DialogSettings" style="display: none;">
    <div class="bd">
        <form action="$Env{"CGIHandle"}" method="post" id="DialogSettingsForm" name="DialogSettingsForm">
            <input type="hidden" name="Action" value="AgentPreferences"/>
            <input type="hidden" name="Subaction" value="Update"/>
            <input type="hidden" name="Group" value="$QData{"Group"}"/>
            <input type="hidden" name="RedirectURL" value="$QData{"RequestedURL"}"/>
            <table>
                <tr valign="top" align="left">
                    <td>
                        <label for="$QData{"PreferencesKey"}"><b>$Text{"Max. shown Tickets a page"}:</b></label>
                        $Data{"PageShownString"}
                    </td>
                </tr>
            </table>
        </form>
    </div>
</div>
<!-- view settings end -->
<!-- dtl:block:DISABLE -->

<!-- dtl:block:OverviewNavBar -->
<div class="OverviewBox ARIARoleMain" id="Main">
    <h1>$Text{"$QData{"TitleName"}"}: $Text{"$QData{"TitleValue"}"}</h1>

    <div class="OverviewControl" id="OverviewControl">

        <div class="ControlRow">
<!-- dtl:block:OverviewNavBarFilter -->
            <ul class="Tabs">
<!-- dtl:block:OverviewNavBarFilterItem -->
<!-- dtl:block:OverviewNavBarFilterItemSelected -->
                <li class="Active $QData{"CSS"}">
                    <a name="OverviewControl" href="$Env{"Baselink"}Action=$Env{"Action"};$Data{"FilterLink"}Filter=$LQData{"Filter"}">$Text{"$QData{"Name"}"}<span>$QData{"Count"}</span></a>
                </li>
<!-- dtl:block:OverviewNavBarFilterItemSelected -->
<!-- dtl:block:OverviewNavBarFilterItemSelectedNot -->
                <li class="$QData{"CSS"}">
                    <a name="OverviewControl" href="$Env{"Baselink"}Action=$Env{"Action"};$Data{"FilterLink"}Filter=$LQData{"Filter"}">$Text{"$QData{"Name"}"}<span>$QData{"Count"}</span></a>
                </li>
<!-- dtl:block:OverviewNavBarFilterItemSelectedNot -->
<!-- dtl:block:OverviewNavBarFilterItemSplit -->
                -
<!-- dtl:block:OverviewNavBarFilterItemSplit -->
<!-- dtl:block:OverviewNavBarFilterItem -->
            </ul>
<!-- dtl:block:OverviewNavBarFilter -->
            <ul class="ContextFunctions">
                <li class="ContextHelp">
                    <a href="" title="Context Help"><span>Context Help</span></a>
                </li>
                <li class="ContextSettings">
                    <a href="" title="Context Settings"><span>Context Settings</span></a>
                </li>
#<!-- view settings start -->
#                            <a href="" id="DialogSet" onclick="return false;" title="$Text{"Overview Settings"}"><img border="0" src="$Config{"Frontend::ImagePath"}edit-inactive.png" /></a>
#                            <img src="$Config{"Frontend::ImagePath"}h-line-small.png" />
#<!-- view settings end -->
            </ul>
        </div>
        <div class="ActionRow">
            <div class="OverviewActions">
                $Data{"ActionRow"}
#            <ul class="Actions">
#                <li><a href="">Multiple Action</a></li>
#            </ul>
                <ul class="OverviewZoom">
<!-- dtl:block:OverviewNavBarViewMode -->
<!-- dtl:block:OverviewNavBarViewModeSelected -->
                    <li class="Active">
                        <a class="$QData{"Name"}" name="OverviewControl" href="$Env{"Baselink"}Action=$Env{"Action"};Filter=$LQData{"Filter"};View=$LQData{"View"};$Data{"LinkFilter"}" title="$Text{"$QData{"Name"}"}"><span>$Text{"$QData{"NameShort"}"}</span></a>
                    </li>
<!-- dtl:block:OverviewNavBarViewModeSelected -->
<!-- dtl:block:OverviewNavBarViewModeNotSelected -->
                    <li class="">
                        <a class="$QData{"Name"}" name="OverviewControl" href="$Env{"Baselink"}Action=$Env{"Action"};Filter=$LQData{"Filter"};View=$LQData{"View"};$Data{"LinkFilter"}" title="$Text{"$QData{"Name"}"}"><span>$Text{"$QData{"NameShort"}"}</span></a>
                    </li>
<!-- dtl:block:OverviewNavBarViewModeNotSelected -->
<!-- dtl:block:OverviewNavBarViewMode -->
                </ul>
#            <form class="Pagination" action="">
#                <a href="" class="ArrowLeft" title="Previous Page"><span>Previous</span></a>
#                <span><input type="text" id="Page"/>/ 9</span>
#                <a href="" class="ArrowRight" title="Next Page"><span>Next</span></a>
#            </form>
<!-- dtl:block:OverviewNavBarPageBack -->
                <a name="OverviewControl" href="$Env{"Baselink"}Action=$Env{"Action"};$Data{"LinkBack"}">$Text{"Change search options"}</a>
<!-- dtl:block:OverviewNavBarPageBack -->
                <div class="Pagination">
<!-- dtl:block:OverviewNavBarPageNavBar -->
                    $Data{"Result"} $Text{"of"} $Data{"TotalHits"} -
                    $Text{"Page"}: $Data{"SiteNavBar"}
<!-- dtl:block:OverviewNavBarPageNavBar -->
                </div>
                <div class="ClearLeft"></div>
            </div>
# sort order bar if needed
            $Data{"SortOrderBar"}
        </div>
    </div>

<!-- dtl:block:OverviewNavBar -->

<a name="subnav"></a>
<!-- dtl:block:OverviewNavBar -->
<table border="0" width="100%" cellspacing="0" cellpadding="3">
  <tr>
    <td class="mainhead">
      $Env{"Box0"}$Text{"$QData{"TitleName"}"}: $Text{"$QData{"TitleValue"}"}$Env{"Box1"}
    </td>
  </tr>
<!-- dtl:block:OverviewNavBarMain -->
  <tr>
    <td class="mainbody">
      $Text{"$QData{"MainName"}"}: $Data{"MainContent"}
    </td>
  </tr>
<!-- dtl:block:OverviewNavBarMain -->
  <tr>
    <td class="menu">
      <table border="0" width="100%" cellspacing="0" cellpadding="0">
        <tr>
          <td width="90%">
            <table border="0" width="100%" cellspacing="0" cellpadding="0">
              <tr>
                <td width="100%">
<!-- dtl:block:OverviewNavBarFilter -->
                  $Text{"Filter"}:
<!-- dtl:block:OverviewNavBarFilterItem -->
<!-- dtl:block:OverviewNavBarFilterItemSelected -->
                  <a href="$Env{"Baselink"}Action=$Env{"Action"}&$Data{"FilterLink"}Filter=$LQData{"Filter"}" style="text-decoration:none"><b>$Text{"$QData{"Name"}"} ($QData{"Count"})</b></a>
<!-- dtl:block:OverviewNavBarFilterItemSelected -->
<!-- dtl:block:OverviewNavBarFilterItemSelectedNot -->
                  <a href="$Env{"Baselink"}Action=$Env{"Action"}&$Data{"FilterLink"}Filter=$LQData{"Filter"}">$Text{"$QData{"Name"}"} ($QData{"Count"})</a>
<!-- dtl:block:OverviewNavBarFilterItemSelectedNot -->
<!-- dtl:block:OverviewNavBarFilterItemSplit -->
                  -
<!-- dtl:block:OverviewNavBarFilterItemSplit -->
<!-- dtl:block:OverviewNavBarFilterItem -->
<!-- dtl:block:OverviewNavBarFilter -->
                </td>
              </tr>
              <tr>
                <td width="100%">
<!-- dtl:block:OverviewNavBarPageBack -->
                  <a href="$Env{"Baselink"}Action=$Env{"Action"}&$Data{"LinkBack"}">$Text{"Change search options"}</a>
<!-- dtl:block:OverviewNavBarPageBack -->
<!-- dtl:block:OverviewNavBarPageNavBar -->
                  $Text{"Tickets"}: $Data{"Result"} $Text{"of"} $Data{"TotalHits"} -
                  $Text{"Page"}: $Data{"SiteNavBar"}
<!-- dtl:block:OverviewNavBarPageNavBar -->
                </td>
              </tr>
            </table>
          </td>
          <td width="20%" align="right" valign="top">
<!-- dtl:block:OverviewNavBarViewMode -->
<!-- dtl:block:OverviewNavBarViewModeSelected -->
            <a href="$Env{"Baselink"}Action=$Env{"Action"}&Filter=$LQData{"Filter"}&View=$LQData{"View"}&$Data{"LinkFilter"}#subnav" Title="$QData{"Name"}"><img border="0" src="$Env{"Images"}$QData{"ImageSelected"}" $QData{"Name"}/></a>
<!-- dtl:block:OverviewNavBarViewModeSelected -->
<!-- dtl:block:OverviewNavBarViewModeNotSelected -->
            <a href="$Env{"Baselink"}Action=$Env{"Action"}&Filter=$LQData{"Filter"}&View=$LQData{"View"}&$Data{"LinkFilter"}#subnav" Title="$QData{"Name"}"><img border="0" src="$Env{"Images"}$QData{"Image"}" title="$QData{"Name"}" /></a>
<!-- dtl:block:OverviewNavBarViewModeNotSelected -->
<!-- dtl:block:OverviewNavBarViewMode -->
            &nbsp;
          </td>
        </tr>
      </table>
    </td>
  </tr>
</table>
<!-- dtl:block:OverviewNavBar -->

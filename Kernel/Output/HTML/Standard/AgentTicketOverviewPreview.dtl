                    </tr>
                    <tr>
                        <td>
                            <label>$Text{"Created"}</label>$TimeLong{"$Data{"Created"}"}
                        </td>
                        <td>
                            <label>$Text{"State"}</label><div title="$Quote{"$Text{"$Data{"State"}"}"}">$Quote{"$Text{"$Data{"State"}"}","26"}</div>

    <!-- dtl:block:ConfigElementTimeVacationDaysOneTime -->
    <td class="contentvalue">
      <table border="0" cellpadding="0" cellspacing="3">
        <tr>
          <td class="contentkey">yyyy</td>
          <td>&nbsp;</td>
          <td class="contentkey">mm</td>
          <td>&nbsp;</td>
          <td class="contentkey">tt</td>
          <td>&nbsp;</td>
          <td class="contentkey">$Text{"Text"}</td>
        </tr>
        <!-- dtl:block:ConfigElementTimeVacationDaysOneTimeContent -->
        <tr>
          <td class="contentvalue"><input type="text" name="$Data{"ElementKey"}year[]" value="$QData{"Year"}" size="4" maxlength="4"></td>
          <td>-</td>
          <td class="contentvalue"><input type="text" name="$Data{"ElementKey"}month[]" value="$QData{"Month"}" size="2" maxlength="2"></td>
          <td>-</td>
          <td class="contentvalue"><input type="text" name="$Data{"ElementKey"}day[]" value="$QData{"Day"}" size="2" maxlength="2"></td>
          <td>&nbsp;</td>
          <td class="contentvalue"><input type="text" name="$Data{"ElementKey"}Content[]" value="$QData{"Content"}" size="20"></td>

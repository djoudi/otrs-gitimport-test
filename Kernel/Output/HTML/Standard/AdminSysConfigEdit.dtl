<br>
<form action="$Env{"CGIHandle"}" method="post">
<input type="hidden" name="Action" value="$Env{"Action"}">
<input type="hidden" name="Subaction" value="Update">
<input type="hidden" name="SysConfigGroup" value="$Data{"Group"}">
<input type="hidden" name="SysConfigSubGroup" value="$Data{"SubGroup"}">
<table cellspacing="0" cellpadding="4" border="0" width="790">
  <tr class="contenthead">
    <td colspan="3">$Text{"Options "}$Text{"for "}$Text{"Subgroup '"}$Data{"SubGroup"}$Text{"' "}:</td>
  </tr>
  <!-- dtl:block:ConfigElementBlock -->
  <tr class="contentbody">
    <td valign="top"><input type="checkbox" value="1" name="$Data{"ItemKey"}ItemAktiv" $Data{"Valid"} $Data{"Required"}></td>
    <td width="50%"><span class="contentkey$Data{"Validstyle"}">$QData{"ItemKey"}:</span><br><span class="contentvalue$Data{"Validstyle"}">$QData{"Description"}</span></td>
    <!-- dtl:block:ConfigElementString -->
    <td width="50%" class="contentvalue" valign="top"><input name="$Data{"ElementKey"}" value="$QData{"Content"}" size="50"><span class="validity">&nbsp;$QData{"Valid"}</span></td>
    <!-- dtl:block:ConfigElementString -->

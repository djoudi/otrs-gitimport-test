    <td width="50%" class="contentvalue" valign="top"><textarea name="$Data{"ElementKey"}" rows="7" cols="50">$Data{"Content"}</textarea></td>
    <!-- dtl:block:ConfigElementTextArea -->
    <!-- dtl:block:ConfigElementSelect -->
    <td width="50%" class="contentvalue" valign="top">$Data{"Liste"}</td>
    <!-- dtl:block:ConfigElementSelect -->

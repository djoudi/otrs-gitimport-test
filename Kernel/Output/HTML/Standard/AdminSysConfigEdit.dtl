<!-- start form -->
<table border="0" width="100%" cellspacing="0" cellpadding="3" cols="1">
<tr>
  <td class="mainhead">
    $Env{"Box0"}$Text{"SysConfig"}$Env{"Box1"}
  </td>
</tr>
<tr>
  <td colspan="2" class="menu">
    <a href="$Env{"Baselink"}$Env{"LastScreenOverview"}">zur&uuml;ck</a>
  </td>
</tr>
<tr>

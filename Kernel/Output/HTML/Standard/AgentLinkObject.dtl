<!-- dtl:block:LinkOption -->
<table border="0" width="100%" cellspacing="0" cellpadding="3">
  <tr>
    <td class="mainhead">
      $Env{"Box0"}$Text{"Link Object"}$Env{"Box1"}
    </td>
  </tr>
  <tr>
    <td class="menu">
      <a href="$Env{"Baselink"}$Env{"LastScreenView"}" onmouseover="window.status='$Text{"Back"}'; return true;" onmouseout="window.status='';" class="menuitem">$Text{"Back"}</a>
    </td>
  </tr>
</table>
<table border="0" width="100%" cellspacing="0" cellpadding="3">
  <tr>
    <td class="mainbody">
      <br>
      <form name="AgentTicketLink" action="$Env{"CGIHandle"}" method="post">
        <input type="hidden" name="Action" value="$Env{"Action"}">
        <table border="0" width="700" align="center" cellspacing="0" cellpadding="4">
          <tr>
            <td colspan="2" class="contenthead">$Text{"Options"}</td>
          </tr>
          <tr>
            <td class="contentbody">
              <table border="0">
                <tr>
                  <td class="contentkey">$Text{"Link Object"} "$Text{"$QData{"SourceObject"}"}" $Text{"with"}:</td>
                  <td class="contentvalue">$Data{"LinkToObject"}</td>
                </tr>
              </table>
            </td>
          </tr>
          <tr>
            <td class="contentfooter">
              <input class="button" type="submit" value="$Text{"Select"}">
            </td>
          </tr>
        </table>
      </form>
      <br>
    </td>
  </tr>
</table>
<!-- dtl:block:LinkOption -->
<!-- dtl:block:Search -->
<table border="0" width="100%" cellspacing="0" cellpadding="3">
  <tr>
    <td class="mainhead">
      $Env{"Box0"}$Text{"Search"}$Env{"Box1"}
    </td>
  </tr>
</table>
<table border="0" width="100%" cellspacing="0" cellpadding="3">
  <tr>
    <td class="mainbody">
      <br>
      <form name="AgentTicketLink" action="$Env{"CGIHandle"}" method="post">
        <input type="hidden" name="Action" value="$Env{"Action"}">
        <input type="hidden" name="Subaction" value="Search">
        <table border="0" width="700" align="center" cellspacing="0" cellpadding="4">
          <tr>
            <td class="contenthead">$Text{"Options"}</td>
          </tr>
          <tr>
            <td class="contentbody">
              <table border="0">
<!-- dtl:block:SearchItem -->
                <tr>
                  <td class="contentkey">$Text{"$QData{"Text"}"}</td>
                  <td class="contentvalue"><input type="text" name="$QData{"Name"}" value="$QData{"Value"}"></td>
                </tr>
<!-- dtl:block:SearchItem -->
<!-- dtl:block:SearchItemSelect -->
                <tr>
                  <td class="contentkey">$Text{"$QData{"Text"}"}</td>
                  <td class="contentvalue">$Data{"Option"}</td>
                </tr>
<!-- dtl:block:SearchItemSelect -->
              </table>
            </td>
          </tr>
          <tr>
            <td class="contentfooter">
              <input class="button" accesskey="g" type="submit" value="$Text{"Search"}">
            </td>
          </tr>
        </table>
      </form>
      <br>
    </td>
  </tr>
</table>
<!-- dtl:block:Search -->
<!-- dtl:block:SearchResult -->
<table border="0" width="100%" cellspacing="0" cellpadding="3">
  <tr>
    <td class="mainhead">
      $Env{"Box0"}$Text{"Result"}$Env{"Box1"}
    </td>
  </tr>
  <tr>
    <td class="menu">
$Text{"Results"}: <b>$QData{"Result"}</b> - $Text{"Total hits"}: <b>$Data{"TotalHits"}</b>
      - $Text{"Page"}: $Data{"SiteNavBar"}
    </td>
  </tr>
</table>
<table border="0" width="100%" cellspacing="0" cellpadding="3">
  <tr>
    <td class="mainbody">
      <table border="0" width="100%" cellpadding="3">
        <tr>
          <td width="10%">$Text{"Number"}</td>
          <td width="30%">$Text{"Title"}</td>
          <td width="18%">$Text{"Created"}</td>
          <td width="18%">$Text{"Changed"}</td>
          <td width="8%" align="center">$Text{"Normal"}</td>
          <td width="8%" align="center">$Text{"Parent"}</td>
          <td width="8%" align="center">$Text{"Child"}</td>
        </tr>
<!-- dtl:block:SearchResultItem -->
        <dtl if ($Data{"Linked"} eq "1") { $Data{"Color"} = "bgcolor="red""; }>
        <dtl if ($Data{"LinkedParent"} eq "1") { $Data{"LinkParent"} = "Unlink"; }>
        <dtl if ($Data{"LinkedParent"} eq "0") { $Data{"LinkParent"} = "Link"; }>
        <dtl if ($Data{"LinkedParent"} eq "2") { $Data{"LinkParent"} = "-"; }>
        <dtl if ($Data{"LinkedChild"} eq "1") { $Data{"LinkChild"} = "Unlink"; }>
        <dtl if ($Data{"LinkedChild"} eq "2") { $Data{"LinkChild"} = "-"; }>
        <dtl if ($Data{"LinkedChild"} eq "0") { $Data{"LinkChild"} = "Link"; }>
        <dtl if ($Data{"LinkedNormal"} eq "1") { $Data{"LinkNormal"} = "Unlink"; }>
        <dtl if ($Data{"LinkedNormal"} eq "2") { $Data{"LinkNormal"} = "-"; }>
        <dtl if ($Data{"LinkedNormal"} eq "0") { $Data{"LinkNormal"} = "Link"; }>
        <dtl if ($Data{"LinkedParent"} eq "1") { $Data{"LinkedParentLink"} = " (linked as Parent)"; }>
        <tr $Data{"Color"} class="contentbody">
          <td><a href="$Env{"Baselink"}Action=$Env{"Action"}&PreviewID=$QData{"ID"}">$QData{"Number"}</a></td>
          <td>$QData{"Title"}</td>
          <td>$TimeLong{"$Data{"Created"}"}</td>
          <td>$TimeLong{"$Data{"Changed"}"}</td>
          <td align="center"><a href="$Env{"Baselink"}Action=$Env{"Action"}&Subaction=$QData{"LinkNormal"}Normal&ID=$QData{"ID"}&StartHit=$QData{"StartHit"}">$Text{"$QData{"LinkNormal"}"}</a></td>
          <td align="center"><a href="$Env{"Baselink"}Action=$Env{"Action"}&Subaction=$QData{"LinkParent"}Parent&ID=$QData{"ID"}&StartHit=$QData{"StartHit"}">$Text{"$QData{"LinkParent"}"}</a></td>
          <td align="center"><a href="$Env{"Baselink"}Action=$Env{"Action"}&Subaction=$QData{"LinkChild"}Child&ID=$QData{"ID"}&StartHit=$QData{"StartHit"}">$Text{"$QData{"LinkChild"}"}</a></td>
        </tr>
<!-- dtl:block:SearchResultItem -->
      </table>
    </td>
  </tr>
</table>
<!-- dtl:block:SearchResult -->
<!-- dtl:block:Preview -->
<table border="0" width="100%" cellspacing="0" cellpadding="3">
  <tr>
    <td class="mainhead">
      $Env{"Box0"}$Text{"Preview"}$Env{"Box1"}
    </td>
  </tr>
  <tr>
    <td class="menu">
      <a href="$Env{"Baselink"}$Env{"LastScreenView"}" onmouseover="window.status='$Text{"Detail"}'; return true;" onmouseout="window.status='';" class="menuitem">$Text{"Detail"}</a>
    </td>
  </tr>
</table>
<table border="0" width="100%" cellspacing="0" cellpadding="3">
  <tr>
    <td valign="top" class="mainbody">
      <pre>
        $Data{"Body"}
      </pre>
    </td>
  </tr>
</table>
<!-- dtl:block:Preview-->


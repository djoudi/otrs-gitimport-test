<!-- dtl:block:Link -->
<table border="0" width="100%" cellspacing="0" cellpadding="3">
  <tr>
    <td class="mainhead">
      $Env{"Box0"}$Text{"Link Object: %s","$QData{"SourceItemDescriptionNormal"}$Env{"Box1"}
    </td>
  </tr>
</table>
<table border="0" width="100%" cellspacing="0" cellpadding="3">
  <tr>
    <td width="75%" class="menu">
      <a href="$Env{"Baselink"}$Env{"LastScreenView"}" onmouseover="window.status='$JSText{"Back"}'; return true;" onmouseout="window.status='';" class="menuitem">$Text{"Back"}</a>
      -
      <a href="$Env{"Baselink"}Action=$Env{"Action"}&Subaction=LinkDelete&SourceObject=$LQData{"SourceObject"}&SourceKey=$LQData{"SourceKey"}" class="menuitem">$Text{"Delete"}</a>
    </td>
    <td align="right" width="25%" class="menu"></td>
  </tr>
</table>
<table border="0" width="100%" cellspacing="0" cellpadding="3">
  <tr>
    <td class="mainbody" align="center">
      <br>
      <form action="$Env{"CGIHandle"}" method="get" name="compose">
        <input type="hidden" name="Action" value="$Env{"Action"}">
        <input type="hidden" name="SourceObject" value="$QData{"SourceObject"}">
        <input type="hidden" name="SourceKey" value="$QData{"SourceKey"}">
        <input type="hidden" name="TargetObject" value="$QData{"TargetObject"}">
        <table width="800" border="0" cellspacing="0" cellpadding="3">
          <tr>
            <td class="contenthead">
              $Text{"Object"}
            </td>
          </tr>
          <tr>
            <td class="contentbody">
              $Text{"Link Object"}
              <span title="$QData{"SourceItemDescriptionLong"}">
              $QData{"SourceItemDescriptionNormal"}
              </span>
              $Text{"with"} $Data{"TargetObjectStrg"}
            </td>
          </tr>
          <tr>
            <td class="contentfooter">
              <input class="button" type="submit" value="$Text{"Select"}"/>
            </td>
          </tr>
        </table>
        <br>
        <table width="800" border="0" cellspacing="0" cellpadding="3">
          <tr>
            <td colspan="2" class="contenthead">
              $Text{"Search"} $QData{"TargetRealname"}
            </td>
          </tr>
<!-- dtl:block:LinkSearchRow -->
          <tr class="contentbody">
            <td width="20%" class="contentkey">$Text{"$Data{"Value"}"}: </td>
            <td class="contentvalue">
              <input type="Text" name="$QData{"Key"}" size="60" value="$QData{"SearchParam"}">
            </td>
          </tr>
<!-- dtl:block:LinkSearchRow -->
          <tr>
            <td colspan="2" class="contentfooter">
              <input class="button" type="submit" name="SubmitSearch" value="$Text{"Search"}"/>
            </td>
          </tr>
        </table>
<!-- dtl:block:LinkResult -->
        <br>
        <table width="800" border="0" cellspacing="0" cellpadding="3">
          <tr>
<!-- dtl:block:LinkResultColumn -->
            <td class="contenthead">
              $Text{"$Data{"ColumnDescription"}"}
            </td>
<!-- dtl:block:LinkResultColumn -->
          </tr>
<!-- dtl:block:LinkResultRow -->
          <tr>
<!-- dtl:block:LinkResultRowColumn -->
            <td class="$QData{"CssClass"}">
              $Data{"CellData"}
            </td>
<!-- dtl:block:LinkResultRowColumn -->
          </tr>
<!-- dtl:block:LinkResultRow -->
          <tr>
            <td colspan="$QData{"LinkColspan"}" class="contentfooter">
              $Data{"LinkTypeStrg"}
              &nbsp;
              <input class="button" type="submit" name="SubmitLink" value="$Text{"Link"}"/>
            </td>
          </tr>
        </table>
<!-- dtl:block:LinkResult -->
      </form>
      <br>
    </td>
  </tr>
</table>
<!-- dtl:block:Link -->
<!-- dtl:block:Delete -->
<table border="0" width="100%" cellspacing="0" cellpadding="3">
  <tr>
    <td class="mainhead">
      $Env{"Box0"}$Text{"Unlink Object: %s",$QData{"SourceItemDescriptionNormal"}$Env{"Box1"}
    </td>
  </tr>
</table>
<table border="0" width="100%" cellspacing="0" cellpadding="3">
  <tr>
    <td width="75%" class="menu">
      <a href="$Env{"Baselink"}Action=$Env{"Action"}&SourceObject=$LQData{"SourceObject"}&SourceKey=$LQData{"SourceKey"}" class="menuitem">$Text{"Back"}</a>
    </td>
    <td align="right" width="25%" class="menu"></td>
  </tr>
</table>
<table border="0" width="100%" cellspacing="0" cellpadding="3">
  <tr>
    <td class="mainbody" align="center">
      <br>
<!-- dtl:block:DeleteObject -->
      <form action="$Env{"CGIHandle"}" method="get">
        <input type="hidden" name="Action" value="$Env{"Action"}">
        <input type="hidden" name="Subaction" value="LinkDelete">
        <input type="hidden" name="SourceObject" value="$QData{"SourceObject"}">
        <input type="hidden" name="SourceKey" value="$QData{"SourceKey"}">
        <input type="hidden" name="TargetObject" value="$QData{"TargetObject"}">
        <table width="800" border="0" cellspacing="0" cellpadding="3">
          <tr>
            <td colspan="$QData{"LinkColspan"}">
              <b><i>$QData{"TargetObjectRealname"} $QData{"TargetSubobject"}</i></b>
            </td>
          </tr>
          <tr>
<!-- dtl:block:DeleteObjectLinkColumn -->
            <td class="contenthead">
              $Text{"$Data{"ColumnDescription"}"}
            </td>
<!-- dtl:block:DeleteObjectLinkColumn -->
          </tr>
<!-- dtl:block:DeleteObjectLinkRow -->
          <tr>
<!-- dtl:block:DeleteObjectLinkRowColumn -->
            <td class="$QData{"CssClass"}">
              $Data{"CellData"}
            </td>
<!-- dtl:block:DeleteObjectLinkRowColumn -->
          </tr>
<!-- dtl:block:DeleteObjectLinkRow -->
          <tr>
            <td colspan="$QData{"LinkColspan"}" class="contentfooter">
              <input class="button" type="submit" name="SubmitDelete" value="$Text{"Delete"}"/>
            </td>
          </tr>
        </table>
        <br>
      </form>
<!-- dtl:block:DeleteObject -->
    </td>
  </tr>
</table>
<!-- dtl:block:Delete -->

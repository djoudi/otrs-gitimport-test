<!-- start record -->
<tr valign="top">
 <td class="PriorityID-$Data{"PriorityID"}">
  <a href="$Env{"Baselink"}Action=AgentZoom&TicketID=$Data{"TicketID"}&ArticleID=$Data{"ArticleID"}&QueueID=$Data{"QueueID"}" onmouseover="window.status='$Text{"Zoom"}'; return true;" onmouseout="window.status='';">$Data{"TicketNumber"}</a>
 </td>
 <td class="PriorityID-$Data{"PriorityID"}">
  <font color="red">$QData{"Age"}</font>
 </td>
 <td class="PriorityID-$Data{"PriorityID"}">
   <font color="red">
   <div title="$QData{"From"}">$QData{"From","30"}</div>
   <div title="$QData{"Subject"}">$QData{"Subject","30"}</div>
   </font>
 </td>
 <td class="PriorityID-$Data{"PriorityID"}">
   <font color="red"><div title="$Quote{"$Text{"$Data{"State"}"}"}">$Quote{"$Text{"$Data{"State"}"}","30"}</div></font>
 </td>
 <td class="PriorityID-$Data{"PriorityID"}">
  <font color="red"><div title="$QData{"Queue"}">$QData{"Queue","30"}</div></font>
 </td>
 <td class="PriorityID-$Data{"PriorityID"}">
   <font color="red"><div title="$QData{"Owner"}">$QData{"Owner","30"}</div> <div title="($QData{"UserFirstname"} $QData{"UserLastname"})">($QData{"UserFirstname","15"} $QData{"UserLastname","15"})</div></font>
 </td>
 <td class="PriorityID-$Data{"PriorityID"}">
  <a href="$Env{"Baselink"}Action=AgentCustomer&TicketID=$QData{"TicketID"}" onmouseover="window.status='$Text{"Customer history"}'; return true;" onmouseout="window.status='';"><div title="$Data{"CustomerID"}">$Quote{"$Data{"CustomerID"}","15"}</div></a>
  <div title="$QData{"CustomerName"}">$QData{"CustomerName","15"}</div>
 </td>  
</tr>
<!-- end record -->

<!-- start form -->
<table border="0" width="100%" cellspacing="0" cellpadding="3" cols="2">
<tr bgcolor="$Env{"BGTableColor0"}">
  <td align="left" width="50%">
  <b><font color="$Env{"FontColor1"}">$Env{"Box0"}$Config{"TicketHook"}: $Data{"TicketNumber"}$Env{"Box1"}</font></b>
  </td>
  <td align="right" width="50%">
  <b><font color="$Env{"FontColor1"}">$Env{"Box0"}$Text{"Age"}: $Data{"Age"}$Env{"Box1"}</font></b></font></b>
  </td>
</tr>
</table>

<table border="0" width="100%" cellspacing="0" cellpadding="3" cols="2">
<tr bgcolor="$Env{"BGTableColor2"}">
  <td align="left" valign="top" width="70%">
    <a href="$Env{"Baselink"}Action=AgentZoom&TicketID=$Data{"TicketID"}&ArticleID=$Data{"ArticleID"}&QueueID=$Data{"QueueID"}" onmouseover="window.status='$Text{"Zoom"}'; return true;" onmouseout="window.status='';">$Text{"Zoom"}</a> -
    <a href="$Env{"Baselink"}Action=AgentHistory&TicketID=$Data{"TicketID"}&QueueID=$Data{"QueueID"}" onmouseover="window.status='$Text{"History"}'; return true;" onmouseout="window.status='';">$Text{"History"}</a> 
  </td>

  <td align="right" valign="top" width="30%">
    <b>$Text{"Created"}:</b> $Data{"Created"}
  </td>
</tr>
</table>

<table border="0" width="100%" cellspacing="0" cellpadding="3" cols="2">
<tr bgcolor="$Env{"BGTableColor1"}">
  <td align="left" valign="top" width="75%">
  <table border="0" width="100%" cellspacing="0" cellpadding="1">
    <tr>
      <td width="15%"><b>$Text{"From"}:</b></td><td> $Data{"From"}</td>
    </tr>
    <tr> 
      <td><b>$Text{"To"}:</b></td><td> $Data{"To"}</td>
    </tr>
    <tr>
      <td><b>$Text{"Subject"}:</b></td><td> $Data{"Subject"}</td>
    </tr>
  </table>
$Data{"TextNote"}
<pre>
$Data{"Body"}
</pre>
  </td> 
  <td align="left" valign="top" bgcolor="$Env{"BGTableColor2"}" width="25%">
    <table border="0">
    <tr><td><b>$Text{"State"}:</b></td><td><font color="red">$Data{"State"}</font></td></tr>
    <tr><td><b>$Text{"Priority"}:</b></td><td><font color="red">$Data{"Priority"}</font></td></tr>
    <tr><td><b>$Text{"Queue"}:</b></td><td><font color="red">$Data{"Queue"}</font></td></tr>
    <tr><td><b>$Text{"Lock"}:</b></td><td>$Data{"Lock"}</td></tr>
    <tr><td><b>$Text{"CustomerID"}:</b></td><td>$Data{"CustomerID"}</td></tr>
    </table>
  </td>
</tr>
</table>
<!-- end form -->


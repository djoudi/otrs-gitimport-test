<!-- start form -->
<table border="0" width="100%" cellspacing="0" cellpadding="3">
    <tr>
        <td class="mainhead">
            $Env{"Box0"}$Text{"Overview"}: $Text{"Stats"}$Env{"Box1"}
        </td>
    </tr>
    <tr>
        <td class="menu">
            $Text{"Results"}: <b>$QData{"Result"}</b> - $Text{"Total hits"}: <b>$QData{"TotalHits"}</b> - $Text{"Page"}: $Data{"SiteNavBar"}
        </td>
    </tr>
    <tr>
        <td class="mainbody">
            <table border="0" width="100%" cellpadding="4" cellspacing="0">
                <tr><td></td></tr>
                <tr class="contenthead">
                    <td>$Text{"$Config{"Stats::StatsHook"}"}
                        <a href="$Env{"Baselink"}Action=$Env{"Action"};Subaction=Overview;OrderBy=ID;Direction=ASC;StartHit=$LQData{"StartHit"}"><img border="0" src="$Config{"Frontend::ImagePath"}down-small.png" alt="$Text{"down"}"/></a>&nbsp;/
                        <a href="$Env{"Baselink"}Action=$Env{"Action"};Subaction=Overview;OrderBy=ID;Direction=DESC;StartHit=$LQData{"StartHit"}"><img border="0" src="$Config{"Frontend::ImagePath"}up-small.png" alt="$Text{"up"}" /></a>
                    </td>
                    <td>$Text{"Title"}
                        <a href="$Env{"Baselink"}Action=$Env{"Action"};Subaction=Overview;OrderBy=Title;Direction=ASC;StartHit=$LQData{"StartHit"}"><img border="0" src="$Config{"Frontend::ImagePath"}down-small.png" alt="$Text{"down"}" /></a>&nbsp;/
                        <a href="$Env{"Baselink"}Action=$Env{"Action"};Subaction=Overview;OrderBy=Title;Direction=DESC;StartHit=$LQData{"StartHit"}"><img border="0" src="$Config{"Frontend::ImagePath"}up-small.png" alt="$Text{"up"}" /></a>
                    </td>
                    <td>$Text{"Object"}
                        <a href="$Env{"Baselink"}Action=$Env{"Action"};Subaction=Overview;OrderBy=Object;Direction=ASC;StartHit=$LQData{"StartHit"}"><img border="0" src="$Config{"Frontend::ImagePath"}down-small.png" alt="$Text{"down"}" /></a>&nbsp;/
                        <a href="$Env{"Baselink"}Action=$Env{"Action"};Subaction=Overview;OrderBy=Object;Direction=DESC;StartHit=$LQData{"StartHit"}"><img border="0" src="$Config{"Frontend::ImagePath"}up-small.png" alt="$Text{"up"}" /></a>
                    </td>
                    <td>$Text{"Description"}</td>
                </tr>
<!-- dtl:block:Result -->
                <tr class="$QData{"css"}">
                    <td class="$QData{"TextCss"}">
                        <a href="$Env{"Baselink"}Action=$Env{"Action"};Subaction=View;StatID=$Data{"StatID"}">
                            $Data{"StatNumber"}
                        </a>
                    </td>
                    <td class="contentvalue">$QData{"Title"}</td>
                    <td class="contentvalue">$Text{"$Data{"ObjectName"}"}</td>
                    <td class="contentvalue">$QData{"Description","50"}</td>
                </tr>
<!-- dtl:block:Result -->
                <tr class="contentfooter">
                    <td colspan="4">&nbsp;</td>
                </tr>
            </table>
            <br/>
        </td>
    </tr>
</table>
<!-- end form -->

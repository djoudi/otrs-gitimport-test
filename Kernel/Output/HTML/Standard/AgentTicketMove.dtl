<script type="text/javascript">//<![CDATA[

function change_selected(sel) {
    start_elements = (sel + 7);
    if (document.compose.elements[0].name == '$Config{"SessionName"}') {
        start_elements = start_elements + 1;
    }
    else if (document.compose.elements[0].name == "ChallengeToken") {
        start_elements = start_elements + 1;
    }

    if (!document.compose.elements[start_elements].checked) {
        document.compose.elements[start_elements].checked = true;
    }
}

function submit_compose() {
    // if RichText is enabled, update the hidden textarea now for the body content check to work correctly
    if (typeof RichTextUpdateLinkedField == 'function') {
        RichTextUpdateLinkedField();
    }

    if (document.compose.DestQueueID.value == "") {
        alert('$JSText{"New Queue"}!');
        document.compose.DestQueueID.focus();
        return false;
    }
    else if (document.compose.Subject.value == "") {
        alert('$JSText{"A message should have a subject!"}');
        document.compose.Subject.focus();
        return false;
    }
    else if (document.compose.Body.value != "" && document.compose.Subject.value == "") {
        alert('$JSText{"A message should have a subject!"}');
        document.compose.Subject.focus();
        return false;
    }
    else if (document.compose.Body.value == "" && document.compose.Subject.value != "") {
        alert('$JSText{"A message should have a body!"}');
        if (typeof RichTextUpdateLinkedField == 'function') {
            RichTextFocus();
        }
        else {
            document.compose.Body.focus();
        }
        return false;
    }
    else if (document.compose.TimeUnits && document.compose.TimeUnits.value > 0 && document.compose.Subject.value == "") {
        alert('$JSText{"If you want to account time, please provide Subject and Text!"}');
        document.compose.Subject.focus();
        return false;
    }
<!-- dtl:block:TicketFreeTextCheckJs -->
    if (document.compose.$Data{"TicketFreeTextField"}.value == "") {
        alert('$JSText{"A required field is:"} ' + document.compose.$Data{"TicketFreeKeyField"}.value + '!');
        document.compose.$Data{"TicketFreeTextField"}.focus();
        return false;
    }
<!-- dtl:block:TicketFreeTextCheckJs -->
<!-- dtl:block:TicketFreeTimeCheckJs -->
    if (document.compose.$Data{"TicketFreeTimeCheck"} && document.compose.$Data{"TicketFreeTimeCheck"}.checked == false) {
        alert('$JSText{"A required field is:"} $QData{"TicketFreeTimeKey"}!');
        document.compose.$Data{"TicketFreeTimeCheck"}.focus();
        return false;
    }
<!-- dtl:block:TicketFreeTimeCheckJs -->
<!-- dtl:block:TimeUnitsJs -->
    else if (document.compose.TimeUnits.value != "") {
        var reg = /^-{0,1}\d+?((\.|,){0,1}\d+?){0,1}$/;
        if (reg.test(document.compose.TimeUnits.value) == false) {
            alert('$JSText{"Invalid time!"}');
            document.compose.TimeUnits.focus();
            return false;
        }
    }
    else if (document.compose.TimeUnits.value == "" && $Config{"Ticket::Frontend::NeedAccountedTime"}) {
        alert('$JSText{"You need to account time!"}');
        document.compose.TimeUnits.focus();
        return false;
    }
<!-- dtl:block:TimeUnitsJs -->
}

function SpellCheckSubmit() {
    document.spelling.Body.value = document.compose.Body.value;
    window.open('', 'spelling', 'toolbar=no,location=no,status=yes,scrollbars=yes,resizable=yes,width=700,height=550,left=100,top=100');
    document.spelling.submit()
}
//]]></script>

<form action="$Env{"CGIHandle"}" method="post" enctype="multipart/form-data" name="compose">
    <input type="hidden" name="Action"       value="$Env{"Action"}"/>
    <input type="hidden" name="FormID"       value="$QData{"FormID"}"/>
    <input type="hidden" name="TicketID"     value="$QData{"TicketID"}"/>
    <input type="hidden" name="TicketUnlock" value="$QData{"TicketUnlock"}"/>
    <input type="hidden" name="OwnerAll"     value="$QData{"OwnerAll"}"/>
    <input type="hidden" name="NoSubmit"     value=""/>

    <div class="WidgetPopup ARIARoleMain">
        <div class="Header">
            <h1>$Text{"Move Ticket"}: $Data{"TicketNumber"}</h1>
            <p>
                <a href="">$Text{"Cancel & close window"}</a>
            </p>

<!-- dtl:block:PropertiesLock -->
            <img border="0" src="$Config{"Frontend::ImagePath"}warning.png" alt="$Text{"Info"}" />: <font color="red">$Text{"Ticket locked!"}</font>
            (<a href="$Env{"Baselink"}Action=AgentTicketLock;Subaction=Unlock;TicketID=$Data{"TicketID"}" onmouseover="window.status='$JSText{"Ticket unlock!"}'; return true;" onmouseout="window.status=''; ">$Text{"Ticket unlock!"}</a>)
<!-- dtl:block:PropertiesLock -->

        </div>
        <div class="Content">

            <div class="Header">
                <h2>$Text{"Options"}</h2>
            </div>

            <div class="Content">
                <fieldset class="TableLike">
                    <label for="DestQueueID">$Text{"New Queue"}:</label>
                    <div class="Field">
                        $Data{"MoveQueuesStrg"}
                    </div>
                    <div class="Clear"></div>

                    <label for="NewUserID">$Text{"New Owner"}:</label>
                    <div class="Field">
                        <input type="radio" name="UserSelection" value="New" $QData{"UserSelection::New"}/>
                        $Data{"OwnerStrg"}
                        <a href="" onclick="change_selected(0); document.compose.OwnerAll.value='3'; Core.AJAXUpdate('AJAXUpdate', 'OwnerAll', ['TicketID', 'DestQueueID', 'NewStateID', 'OwnerAll'], ['NewUserID']); return false;" onmouseout="window.status='';" onmouseover="window.status='$JSText{"All Agents"}'; return true;">
                            $Text{"All"}
                        </a>
                    </div>
                    <div class="Clear"></div>

                    <label for="OldUserID">$Text{"Previous Owner"}:</label>
                    <div class="Field">
                        <input type="radio" name="UserSelection" value="Old" $QData{"UserSelection::Old"}/>
                        $Data{"OldUserStrg"}
                    </div>
                    <div class="Clear"></div>

                    <label for="Subject">$Text{"Subject"}:</label>
                    <div class="Field">
                        <input type="text" id="Subject" name="Subject" value="$QData{"Subject"}" class="W75pc"/>
                        <span class="Error Small">$Text{"$Data{"Subject invalid"}"}</span>
                    </div>
                    <div class="Clear"></div>

                    <label>$Text{"Options"}:</label>
                    <div class="Field">

<!-- dtl:block:SpellCheck -->
                        <a href="" onclick="SpellCheckSubmit(); return false;" onmouseover="window.status='$JSText{"Spell Check"}'; return true;" onmouseout="window.status='';">
                            <img border="0" src="$Config{"Frontend::ImagePath"}spell.png" />
                            $Text{"Spell check"}
                        </a>
<!-- dtl:block:SpellCheck -->

                    </div>
                    <div class="Clear"></div>

                    <label for="RichText">$Text{"Text"}:</label>
                    <div class="Clear"></div>

<!-- dtl:block:RichText -->
$Include{"RichTextEditor"}
<!-- dtl:block:RichText -->

                    <textarea id="RichText" class="RichText Wrap_$Env{"BrowserWrap"}" name="Body" rows="15" cols="$Config{"Ticket::Frontend::TextAreaNote"}">$QData{"Body"}</textarea>
                    <span class="Error Small">$Text{"$Data{"Subject invalid"}"}</span>
                    <div class="Clear"></div>

                    <label>$Text{"Attachment"}:</label>
                    <div class="Field">
                        <table>

<!-- dtl:block:Attachment -->
                            <tr>
                                <td>$QData{"Filename"}</td>
                                <td>$QData{"Filesize"}</td>
                                <td align="right">
                                    <button type="submit" name="AttachmentDelete$QData{"FileID"}" value="$Text{"Delete"}">$Text{"Delete"}</button>
                                </td>
                            </tr>
<!-- dtl:block:Attachment -->

                            <tr>
                                <td colspan="2">
                                    <input name="file_upload" type="file" size="40" class="fixed"/>
                                </td>
                                <td align="right">
                                    <button type="submit" name="AttachmentUpload" value="$Text{"Attach"}">$Text{"Attach"}</button>
                                    <a name="attachment"></a>
                                </td>
                            </tr>
                        </table>
                    </div>
                    <div class="Clear"></div>

<!-- dtl:block:State -->
                    <label for="NewStateID">$Text{"Next ticket state"}:</label>
                    <div class="Field">
                        $Data{"NextStatesStrg"}
                    </div>
                    <div class="Clear"></div>
<!-- dtl:block:State -->

<!-- dtl:block:Priority -->
                    <label for="NewPriorityID">$Text{"Priority"}:</label>
                    <div class="Field">
                        $Data{"NextPrioritiesStrg"}
                    </div>
                    <div class="Clear"></div>
<!-- dtl:block:Priority -->

<!-- dtl:block:TicketFreeText -->
                    <label>$Data{"TicketFreeKeyField"}:</label>
                    <div class="Field">
                        $Data{"TicketFreeTextField"}
                    </div>
                    <div class="Clear"></div>
<!-- dtl:block:TicketFreeText -->

<!-- dtl:block:TicketFreeTime -->
                    <label>$Data{"TicketFreeTimeKey"}:</label>
                    <div class="Field">
                        $Data{"TicketFreeTime"}
                    </div>
                    <div class="Clear"></div>
<!-- dtl:block:TicketFreeTime -->

<!-- dtl:block:TimeUnits -->
                    <label for="TimeUnits">$Text{"Time units"}$Text{"$Config{"Ticket::Frontend::TimeUnits"}"}:</label>
                    <div class="Field">
                        <input type="text" name="TimeUnits" id="TimeUnits" value="$QData{"TimeUnits"}" size="3"/>
                    </div>
                    <div class="Clear"></div>
<!-- dtl:block:TimeUnits -->

                </fieldset>
            </div>
        </div>
        <div class="Footer">
            <button class="Primary" id="submitRichText" accesskey="g" type="submit" value="$Text{"Submit"}" onclick="return submit_compose();">$Text{"Submit"}</button>
        </div>
    </div>
</form>

# invisible form
<form action="$Env{"CGIHandle"}" method="post" name="spelling" target="spelling">
    <input type="hidden" name="Action" value="AgentSpelling"/>
    <input type="hidden" name="Body" value=""/>
    <input type="hidden" name="spell_lang" value=""/>
</form>

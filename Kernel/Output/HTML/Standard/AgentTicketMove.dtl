<!-- start form -->
<script language="JavaScript" type="text/javascript">
<!--

function change_selected(sel) {
   start_elements = (sel + 5);
   if (document.compose.elements[0].name == "$Config{"SessionName"}") {
       start_elements = start_elements + 1;
   }
   if (!document.compose.elements[start_elements].checked) {
        document.compose.elements[start_elements].checked = true;
   }
}

function submit_compose() {
    if (document.compose.DestQueueID.value == "") {
        alert('$JSText{"Queue"}');
        document.compose.DestQueueID.focus();
        return false;
    }
<!-- dtl:block:TimeUnitsJs -->
    else if (document.compose.TimeUnits.value != "") {
        var reg = /^-{0,1}\d+?((\.|,){0,1}\d+?){0,1}$/;
        if (reg.test(document.compose.TimeUnits.value) == false) {
            alert('$JSText{"Invalid time!"}');
            document.compose.TimeUnits.focus();
            return false;
        }
    }
    else if (document.compose.TimeUnits.value == "" && $Config{"Ticket::Frontend::NeedAccountedTime"}) {
        alert('$JSText{"You need to account time!"}');
        document.compose.TimeUnits.focus();
        return false;
    }
<!-- dtl:block:TimeUnitsJs -->
}
//-->
</script>

<table border="0" width="100%" cellspacing="0" cellpadding="3">
  <tr>
    <td class="contenthead">
      $Env{"Box0"}$Text{"Move Ticket"}: $Data{"TicketNumber"}$Env{"Box1"}
    </td>
  </tr>
$Include{"AgentTicketLocked"}
  <tr>
    <td class="mainbody">
      <br>
      <table border="0" width="800" align="center" cellspacing="0" cellpadding="4">
        <tr>
          <td colspan="2" class="contenthead">$Text{"Options"}</td>
        </tr>
        <tr>
          <td class="contentbody">
#$Data{"TicketUnlock"}
            <form action="$Env{"CGIHandle"}" method="post" name="compose">
              <input type="hidden" name="Action" value="$Env{"Action"}">
              <input type="hidden" name="TicketID" value="$Data{"TicketID"}">
              <input type="hidden" name="TicketUnlock" value="$Data{"TicketUnlock"}">
#              <input type="hidden" name="DestQueueID" value="">
              <input type="hidden" name="ExpandQueueUsers" value="">
              <input type="hidden" name="AllUsers" value="0">
              <table border="0" width="100%">
                <tr>
                  <td class="contentkey">$Text{"New Queue"}:</td>
                  <td class="contentvalue"></td>
                  <td class="contentvalue">$Data{"MoveQueuesStrg"} $Data{"LatestQueue"}</td>
                </tr>
                <tr>
                  <td width="18%" class="contentkey">$Text{"New Owner"}:</td>
                  <td width="2%" class="contentvalue">
                    <input type="radio" name="UserSelection" value="New" checked="checked">
                  </td>
                  <td width="80%" class="contentvalue">
                    $Data{"OwnerStrg"} $Env{"Box0"}<a href="" onclick="document.compose.ExpandQueueUsers.value='3'; document.compose.AllUsers.value='1'; document.compose.submit(); return false;" onmouseout="window.status='';" onmouseover="window.status='$JSText{"All Agents"}'; return true;">$Text{"All"}</a>$Env{"Box1"}
                  </td>
                </tr>
                <tr>
                  <td class="contentkey">$Text{"Previous Owner"}:</td>
                  <td class="contentvalue"><input type="radio" name="UserSelection" value="Old"></td>
                  <td class="contentvalue">$Data{"OldUserStrg"}</td>
                </tr>
                <tr>
                  <td class="contentkey">$Text{"Note"}:</td>
                  <td class="contentvalue"></td>
                  <td class="contentvalue">
                    <textarea name="Comment" rows="8" cols="$Config{"Ticket::Frontend::TextAreaNote"}" wrap="$Env{"BrowserWrap"}">$Data{"Comment"}</textarea>
                  </td>
                </tr>
# --
# use ticket is move
# --
<!-- dtl:block:State -->
                <tr>
                  <td width="20%" class="contentkey">$Text{"Next ticket state"}:</td>
                  <td class="contentvalue"></td>
                  <td width="80%">$Data{"NextStatesStrg"}</td>
                </tr>
<!-- dtl:block:State -->
<!-- dtl:block:TimeUnits -->
                <tr>
                  <td class="contentkey">$Text{"Time units"}$Text{"$Config{"Ticket::Frontend::TimeUnits"}"}:</td>
                  <td class="contentvalue"></td>
                  <td class="contentvalue"><input type="text" name="TimeUnits" value="$QData{"TimeUnits"}" size="3"></td>
                </tr>
<!-- dtl:block:TimeUnits -->
              </table>
              <tr>
                <td class="contentfooter">
                  <input class="button" accesskey="g" type="submit" value="$Text{"Submit"}" onclick="return submit_compose();">
                </td>
              </tr>
            </form>
          </td>
        </tr>
      </table>
      <br>
    </td>
  </tr>
</table>
<!-- end form -->

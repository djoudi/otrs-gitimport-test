<!-- start record -->
<tr>
 <td class="PriorityID-$Data{"PriorityID"}" align="center"><a href="$Env{"Baselink"}Action=AgentZoom&TicketID=$Data{"TicketID"}&QueueID=$Data{"QueueID"}" onmouseover="window.status='$Text{"Zoom"}'; return true;" onmouseout="window.status='';">$Data{"TicketNumber"}</a></td>
 <td class="PriorityID-$Data{"PriorityID"}"><font color="red">$Data{"Age"}</font></td>
 <td class="PriorityID-$Data{"PriorityID"}"><font color="red">$Data{"Subject"}</font>&nbsp;</td>
 <td class="PriorityID-$Data{"PriorityID"}"><font color="red">$Data{"State"}</font></td>
 <td class="PriorityID-$Data{"PriorityID"}"><font color="red">$Data{"Queue"}</font></td>
 <td class="PriorityID-$Data{"PriorityID"}"><font color="red">$Data{"Owner"}</font></td>
 <td class="PriorityID-$Data{"PriorityID"}"><a href="$Env{"Baselink"}Action=AgentCustomer&TicketID=$Data{"TicketID"}" onmouseover="window.status='$Text{"Customer history"}'; return true;" onmouseout="window.status='';">$Data{"CustomerID"}</a>&nbsp;</td>  
</tr>
<!-- end record -->


                    </fieldset>
                </div>
            </div>
        </div>
        <div id="CustomerTickets"></div>
        <div class="Footer">
            <button class="Primary" id="submitRichText" accesskey="g" type="submit" value="$Text{"Create"}">$Text{"Create"}</button>

                        <div class="Field">
                            <input type="text" id="TimeUnits" name="TimeUnits" value="$QData{"TimeUnits"}" size="3" class="$QData{"TimeUnitsRequired"}"/>
                            <div id="TimeUnitsError" class="TooltipErrorMessage">
                                <p>$Text{"Invalid time!"}</p>
                            </div>
                        </div>
                        <div class="Clear"></div>

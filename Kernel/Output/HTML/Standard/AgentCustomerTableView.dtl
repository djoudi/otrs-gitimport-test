<!-- start customer table -->
<table>
<!-- dtl:block:Row -->
<tr>
  <td><b>$Text{"$QData{"Key"}"}:</b></td>
  <td>$Data{"LinkStart"}<div title="$QData{"Value"}">$Data{"ValueShort"}</div>$Data{"LinkStop"}</td>
</tr>
<!-- dtl:block:Row -->
</table>
<!-- stop customer table -->

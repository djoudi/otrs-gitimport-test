<!-- dtl:block:Customer -->
<fieldset class="TableLike FixedLabelSmall Tight">
<!-- dtl:block:CustomerImage -->
<!-- dtl:block:CustomerImageRealname -->
    $QData{"UserFirstname","200"} $QData{"UserLastname","200"}
    <br/>
<!-- dtl:block:CustomerImageRealname -->
    <img border="0" src="$Data{"URL"}" />
    <br/>
<!-- dtl:block:CustomerImage -->
<!-- dtl:block:CustomerRow -->
    <label>$Text{"$Data{"Key"}"}:</label>
    <p class="Value" title="$QData{"Value"}">$Data{"LinkStart"}$Data{"ValueShort"}$Data{"LinkStop"}
<!-- dtl:block:CustomerRowItem -->
#        $Data{"Item"}
        <a href="$Data{"URL"}" target="$QData{"Target"}"><img src="$Config{"Frontend::ImagePath"}$QData{"Image"}" border="0" alt="$Text{"$Data{"Action"}"}" title="$Text{"$Data{"Alt"}"}" /></a>

<!-- dtl:block:CustomerRowItem -->
    </p>
    <div class="Clear"></div>
<!-- dtl:block:CustomerRow -->

    <table width="70%" border="0" cellspacing="3" cellpadding="1">
<!-- dtl:block:CustomerItem-->
        <tr>
<!-- dtl:block:CustomerItemRow-->
            <td width="10%">
                <a href="$Data{"URL"}" target="$QData{"Target"}"><img src="$Config{"Frontend::ImagePath"}$QData{"Image"}" border="0" alt="$Text{"$Data{"Text"}"}" title="$Text{"$Data{"Text"}"}" /></a>
            </td>
            <td>
                <nobr><a href="$Data{"URL"}" target="$QData{"Target"}">$Text{"$Data{"Text"}"}$QData{"Extention"}</a></nobr>
            </td>
<!-- dtl:block:CustomerItemRow-->
        </tr>
<!-- dtl:block:CustomerItem-->
    </table>
</fieldset>
<!-- dtl:block:Customer -->

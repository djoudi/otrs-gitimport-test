<!-- dtl:block:Customer -->
<fieldset class="TableLike FixedLabelSmall Tight">
<!-- dtl:block:CustomerImage -->
<!-- dtl:block:CustomerImageRealname -->
    <p>
        $QData{"UserFirstname","200"} $QData{"UserLastname","200"}
    </p>
<!-- dtl:block:CustomerImageRealname -->
    <div>
        <img src="$Data{"URL"}" />
    </div>
<!-- dtl:block:CustomerImage -->
<!-- dtl:block:CustomerRow -->
    <label>$Text{"$Data{"Key"}"}:</label>
    <p class="Value" title="$QData{"Value"}">
        $Data{"LinkStart"}$Data{"ValueShort"}$Data{"LinkStop"}
<!-- dtl:block:CustomerRowItem -->
#        $Data{"Item"}
        <a href="$Data{"URL"}" target="$QData{"Target"}">
            <img src="$Config{"Frontend::ImagePath"}$QData{"Image"}" border="0" alt="$Text{"$Data{"Action"}"}" title="$Text{"$Data{"Alt"}"}" />
        </a>
<!-- dtl:block:CustomerRowItem -->
    </p>
    <div class="Clear"></div>
<!-- dtl:block:CustomerRow -->

<!-- dtl:block:CustomerItem-->
<!-- dtl:block:CustomerItemRow-->
    <label>
        <a href="$Data{"URL"}" target="$QData{"Target"}" title="$Text{"$Data{"Text"}"}"><img src="$Config{"Frontend::ImagePath"}$QData{"Image"}" border="0" alt="$Text{"$Data{"Text"}"}" title="$Text{"$Data{"Text"}"}" /></a>
    </label>
    <div class="Value">
        <a class="NoWrap" href="$Data{"URL"}" target="$QData{"Target"}">$Text{"$Data{"Text"}"}$QData{"Extention"}</a>
    </div>
    <div class="Clear"></div>
<!-- dtl:block:CustomerItemRow-->
<!-- dtl:block:CustomerItem-->

</fieldset>
<!-- dtl:block:Customer -->

# start generic agent every 20 minutes
*/20 * * * * [ -x $HOME/bin/GenericAgent.pl ] && $HOME/bin/GenericAgent.pl >> /dev/null
# example to execute GenericAgent.pl on 23:00 with
# Kernel::Config::GenericAgentMove job file
#0 23 * * * [ -x $HOME/bin/GenericAgent.pl ] && $HOME/bin/GenericAgent.pl -c "Kernel::Config::GenericAgentMove" >> /dev/null
